    <rdf:type rdf:resource="#UserBehaviorAnalysis"/>
    <d3f:kb-reference rdf:resource="#Reference_AnomalyDetectionUsingAdaptiveBehavioralProfiles_SecuronixInc"/>
  </rdf:Description>
