  <rdf:Description rdf:about="#SenderReputationAnalysis">
    <rdfs:subClassOf rdf:resource="#MessageAnalysis"/>
    <rdf:type rdf:resource="#MessageAnalysis"/>
    <d3f:d3fend-id>D3-SRA</d3f:d3fend-id>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
