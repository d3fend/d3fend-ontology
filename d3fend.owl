  <rdf:Description rdf:about="#MessageAnalysis">
    <d3f:definition>Analyzing email or instant message content to detect unauthorized activity.</d3f:definition>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:subClassOf rdf:resource="#DefensiveTechnique"/>
