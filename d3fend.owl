  <rdf:Description rdf:about="#IPCTrafficAnalysis">
    <d3f:definition>Analyzing standard inter process communication (IPC) protocols to detect deviations from normal protocol activity.</d3f:definition>
    <d3f:kb-reference rdf:resource="#Reference_CAR-2013-05-003:SMBWriteRequest_"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <d3f:kb-reference rdf:resource="#Reference_SecuritySystemWithMethodologyForInterprocessCommunicationControl_CheckPointSoftwareTechInc"/>
