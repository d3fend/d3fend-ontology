    <d3f:kb-reference rdf:resource="#Reference_SystemAndMethodsThereofForDetectionOfPersistentThreatsInAComputerizedEnvironmentBackground_PaloAltoNetworksIncCyberSecdoLtd"/>
    <rdfs:label>Process Spawn Analysis</rdfs:label>
    <rdfs:subClassOf rdf:nodeID="Nb2c0cd8b56a04932ab7ad10987fa9e35"/>
    <d3f:kb-reference rdf:resource="#Reference_CAR-2014-07-001:ServiceSearchPathInterception_MITRE"/>
