    <d3f:synonym>Forward Resolution IP Blacklisting</d3f:synonym>
    <d3f:definition>Blocking a DNS lookup's answer's IP address value.</d3f:definition>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="#DNSDenylisting"/>
    <d3f:blocks rdf:resource="#DNSNetworkTraffic"/>
    <rdfs:subClassOf rdf:nodeID="N7ebe6ebe75e04e80ad9d0e06e225254b"/>
    <rdfs:label>Forward Resolution IP Denylisting</rdfs:label>
