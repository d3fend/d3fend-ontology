    <d3f:has-link rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://patents.google.com/patent/US20150052603A1</d3f:has-link>
    <d3f:kb-author>Kevin Dale Morgan</d3f:kb-author>
