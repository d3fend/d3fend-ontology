  <rdf:Description rdf:about="#SenderMTAReputationAnalysis">
    <d3f:kb-reference rdf:resource="#Reference_SystemsAndMethodsForDetectingAnd/orHandlingTargetedAttacksInTheEmailChannel_GraphusInc"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:subClassOf rdf:resource="#MessageAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
