    <skos:altLabel>Email &amp; Messaging Analysis</skos:altLabel>
    <rdfs:subClassOf rdf:nodeID="N6322a62cdc474c3e8a0ddae584e4f29f"/>
    <rdfs:label>Message Analysis</rdfs:label>
