    <rdfs:label>Message Analysis</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <d3f:d3fend-id>D3-MA</d3f:d3fend-id>
    <d3f:definition>Analyzing email or instant message content to detect unauthorized activity.</d3f:definition>
    <d3f:synonym>Email Or Messaging Analysis</d3f:synonym>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <d3f:synonym>Electronic Message Analysis</d3f:synonym>
    <d3f:enables rdf:resource="#Detect"/>
