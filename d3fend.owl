  <rdf:Description rdf:about="#MessageAuthentication">
    <d3f:authenticates rdf:resource="#UserToUserMessage"/>
    <rdfs:subClassOf rdf:nodeID="N5208db06008c4da3af7f07fe40e414a5"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
