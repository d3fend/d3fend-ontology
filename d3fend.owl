    <d3f:d3fend-id>D3-EHPV</d3f:d3fend-id>
    <d3f:definition>Validates that a referenced exception handler pointer is a valid exception handler.</d3f:definition>
    <rdfs:subClassOf rdf:resource="#ApplicationHardening"/>
    <rdfs:subClassOf rdf:nodeID="Nfe32db9318b34eb8a83f2d5bf05ab774"/>
