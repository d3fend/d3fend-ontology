  <rdf:Description rdf:about="#Reference_CAR-2013-10-001:UserLoginActivityMonitoring_MITRE">
    <d3f:has-link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://car.mitre.org/analytics/CAR-2013-10-001/</d3f:has-link>
    <rdf:type rdf:resource="#ExternalKnowledgeBase"/>
