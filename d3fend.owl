    <rdfs:subClassOf rdf:resource="#PlatformMonitoring"/>
    <d3f:kb-reference rdf:resource="#Reference-FirmwareVerificationTrapezoid"/>
    <d3f:d3fend-id>D3-FV</d3f:d3fend-id>
    <rdf:type rdf:nodeID="Nb48e82bf728844fc816cb03ddeb67afa"/>
