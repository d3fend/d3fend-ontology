  <rdf:Description rdf:about="#Reference_CAR-2019-08-002:ActiveDirectoryDumpingViaNTDSUtil_MITRE">
    <d3f:kb-author>MITRE</d3f:kb-author>
    <d3f:has-link rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://car.mitre.org/analytics/CAR-2019-08-002/</d3f:has-link>
