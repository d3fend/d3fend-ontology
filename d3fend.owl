  <rdf:Description rdf:about="#Reference_CAR-2019-08-002:ActiveDirectoryDumpingViaNTDSUtil_MITRE">
    <d3f:has-link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://car.mitre.org/analytics/CAR-2019-08-002/</d3f:has-link>
    <rdf:type rdf:resource="#ExternalKnowledgeBase"/>
    <d3f:kb-reference-title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CAR-2019-08-002: Active Directory Dumping via NTDSUtil</d3f:kb-reference-title>
    <d3f:kb-author rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MITRE</d3f:kb-author>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reference - CAR-2019-08-002: Active Directory Dumping via NTDSUtil - MITRE</rdfs:label>
    <d3f:kb-is-example-of rdf:resource="#ProcessSpawnAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <d3f:kb-abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The NTDSUtil tool may be used to dump a Microsoft Active Directory database to disk for processing with a credential access tool such as Mimikatz. This is performed by launching ntdsutil.exe as a privileged user with command line arguments indicating that media should be created for offline Active Directory installation and specifying a folder path. This process will create a copy of the Active Directory database, ntds.dit, to the specified folder path.</d3f:kb-abstract>
    <d3f:kb-mitre-analysis rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> </d3f:kb-mitre-analysis>
    <d3f:kb-organization rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MITRE</d3f:kb-organization>
  </rdf:Description>
