    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <d3f:contains rdf:resource="#NetworkPackets"/>
    <rdfs:label>Remote Procedure Call</rdfs:label>
  </rdf:Description>
