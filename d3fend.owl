    <rdf:type rdf:resource="#PatentReference"/>
    <d3f:kb-author>Patrick W. Dowd, John T. McHenry</d3f:kb-author>
    <d3f:kb-reference-title>Firewall for processing a connectionless network packet</d3f:kb-reference-title>
