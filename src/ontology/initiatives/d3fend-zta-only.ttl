@prefix d3f: <http://d3fend.mitre.org/ontologies/d3fend.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

: a owl:Ontology ;
    owl:imports <http://d3fend.mitre.org/ontologies/d3fend.owl> .

### Classes

:AccessMediation a owl:Class ;
    rdfs:subClassOf d3f:DefensiveTechnique,
        [ a owl:Restriction ;
            owl:onProperty d3f:enables ;
            owl:someValuesFrom d3f:Isolate ] .

:ExecutableAllowlisting a owl:Class ;
    rdfs:subClassOf :SystemCallMediation .

:LANAccessMediation a owl:Class ;
    rdfs:subClassOf :NetworkAccessMediation .

:LocalFileAccessMediation a owl:Class ;
    rdfs:subClassOf :SystemCallMediation .

:NetworkAccessMediation a owl:Class ;
    rdfs:subClassOf :AccessMediation .

:NetworkResourceAccessMediation a owl:Class ;
    rdfs:subClassOf :AccessMediation .

:PhysicalAccessMediation a owl:Class ;
    rdfs:subClassOf :AccessMediation .

:RemoteFileAccessMediation a owl:Class ;
    rdfs:subClassOf :NetworkResourceAccessMediation .

:RoutingAccessMediation a owl:Class ;
    rdfs:subClassOf :NetworkAccessMediation .

:SubroutineAccessMediation a owl:Class ;
    rdfs:subClassOf :AccessMediation .

:SystemCallMediation a owl:Class ;
    rdfs:subClassOf :AccessMediation .

:WebServerAccessMediation a owl:Class ;
    rdfs:subClassOf :NetworkResourceAccessMediation .

