@prefix : <http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#> .
@prefix d3f: <http://d3fend.mitre.org/ontologies/d3fend.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#> .

<http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#> rdf:type owl:Ontology ;
                                                                  owl:imports <http://d3fend.mitre.org/ontologies/d3fend.owl> .

#################################################################
#    Object Properties
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#decodes
:decodes rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf d3f:associated-with ;
         rdfs:label "decodes" ,
                    "x decodes y: Entity x transforms data y to a different form, usually through decompression." .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#encodes
:encodes rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf d3f:associated-with ;
         d3f:definition "x encodes y: Entity x transforms data y to a different form, usually through compression." ;
         rdfs:label "encodes" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#enforces
:enforces rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf d3f:associated-with ;
          d3f:definition "x enforces y: Technique x forces entity y to be compliant with a law, rule, or obligation." ;
          rdfs:label "enforces" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#excises
:excises rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf d3f:associated-with ;
         d3f:definition "x excises y: Technique x removes a section of entity y." ;
         rdfs:label "excises" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#quarantines
:quarantines rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf d3f:associated-with ;
             d3f:definition "x quarantines y: Technique x moves entity y to a place of isolation." ;
             rdfs:label "quarantines" .


#################################################################
#    Classes
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#Array
:Array rdf:type owl:Class ;
       rdfs:subClassOf :StructuredDigitalInformationContent ;
       d3f:definition "An array is a systematic arrangement of similar objects, usually in rows and columns." ;
       rdfs:isDefinedBy "https://dbpedia.org/page/Array" ;
       rdfs:label "Array" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#BitmapImage
:BitmapImage rdf:type owl:Class ;
             rdfs:subClassOf :DigitalImage ;
             d3f:definition "A graphical image whose data is stored in a grid format." ;
             rdfs:label "Bitmap Image" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#BitmapImageFile
:BitmapImageFile rdf:type owl:Class ;
                 rdfs:subClassOf :ImageFile ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty d3f:contains ;
                                   owl:someValuesFrom :BitmapImage
                                 ] ;
                 d3f:definition "A file that contains graphics data represented in a bitmap." ;
                 rdfs:label "Bitmap Image File" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#Character
:Character rdf:type owl:Class ;
           rdfs:subClassOf :UnstructuredDigitalInformationContent ;
           d3f:definition "Characters represent a single textual symbol." ;
           rdfs:isDefinedBy "https://dbpedia.org/page/Character_(computing)" ;
           rdfs:label "Character" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#CodecApplication
:CodecApplication rdf:type owl:Class ;
                  rdfs:subClassOf d3f:UserApplication ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty d3f:contains ;
                                    owl:someValuesFrom :CodecLibrary
                                  ] ;
                  d3f:definition "An application that encodes and decodes digital data." ;
                  rdfs:label "Codec Application" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#CodecLibrary
:CodecLibrary rdf:type owl:Class ;
              rdfs:subClassOf d3f:SoftwareLibrary ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :decodes ;
                                owl:someValuesFrom :EncodedData
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :encodes ;
                                owl:someValuesFrom :EncodedData
                              ] ;
              rdfs:label "A software component that encodes or decodes a data stream of signal." ,
                         "Codec Library" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#ContentExcision
:ContentExcision rdf:type owl:Class ;
                 rdfs:subClassOf :ContentModification ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :excises ;
                                   owl:someValuesFrom :Data
                                 ] .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#ContentFiltering
:ContentFiltering rdf:type owl:Class ;
                  rdfs:subClassOf d3f:DefensiveTechnique ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :enforces ;
                                    owl:someValuesFrom :ContentPolicy
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty d3f:enables ;
                                    owl:someValuesFrom d3f:Isolate
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty d3f:filters ;
                                    owl:someValuesFrom d3f:File
                                  ] .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#ContentFormatConversion
:ContentFormatConversion rdf:type owl:Class ;
                         rdfs:subClassOf :ContentModification .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#ContentModification
:ContentModification rdf:type owl:Class ;
                     rdfs:subClassOf :ContentFiltering ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty d3f:modifies ;
                                       owl:someValuesFrom d3f:File
                                     ] .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#ContentPolicy
:ContentPolicy rdf:type owl:Class ;
               rdfs:subClassOf d3f:DigitalInformationBearer ;
               d3f:definition "A set of rules and guidelines that dictate the acceptable use, distribution, and management of digital content within a system or platform. It defines what content is allowed, restricted, or prohibited, ensuring compliance with legal, ethical, and organizational standards." ;
               rdfs:label "Content Policy" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#ContentQuarantine
:ContentQuarantine rdf:type owl:Class ;
                   rdfs:subClassOf :ContentFiltering ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :quarantines ;
                                     owl:someValuesFrom d3f:File
                                   ] .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#ContentRebuild
:ContentRebuild rdf:type owl:Class ;
                rdfs:subClassOf :ContentModification ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty d3f:creates ;
                                  owl:someValuesFrom d3f:File
                                ] .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#ContentSubstitution
:ContentSubstitution rdf:type owl:Class ;
                     rdfs:subClassOf :ContentModification .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#ContentValidation
:ContentValidation rdf:type owl:Class ;
                   rdfs:subClassOf :ContentFiltering ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty d3f:analyzes ;
                                     owl:someValuesFrom d3f:FileSection
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty d3f:verifies ;
                                     owl:someValuesFrom :StructuredDigitalInformationContent
                                   ] .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#Data
:Data rdf:type owl:Class ;
      rdfs:subClassOf d3f:DigitalInformation ;
      d3f:definition "A collection of discrete values that convey information." ;
      rdfs:isDefinedBy "https://dbpedia.org/page/Data" ;
      rdfs:label "Data" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#DecoderApplication
:DecoderApplication rdf:type owl:Class ;
                    rdfs:subClassOf :CodecApplication ;
                    d3f:definition "An application that decodes digital data." ;
                    rdfs:label "Decoder Application" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#Dictionary
:Dictionary rdf:type owl:Class ;
            rdfs:subClassOf :StructuredDigitalInformationContent ;
            rdfs:isDefinedBy "A dictionary is an data type that stores a collection of (key, value) pairs, such that each possible key appears at most once in the collection." ,
                             "https://dbpedia.org/page/Associative_array" ;
            rdfs:label "Dictionary" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#DigitalAudio
:DigitalAudio rdf:type owl:Class ;
              rdfs:subClassOf :DigitalMedia ;
              d3f:definition "Digital audio is a representation of sound recorded in, or converted into, digital form." ;
              rdfs:isDefinedBy "https://dbpedia.org/page/Digital_audio" ;
              rdfs:label "Digital Audio" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#DigitalAudioVisualMedia
:DigitalAudioVisualMedia rdf:type owl:Class ;
                         rdfs:subClassOf :DigitalMultimedia ;
                         d3f:definition "Audiovisual (AV) is electronic media possessing both a sound and a visual component." ;
                         rdfs:isDefinedBy "https://dbpedia.org/page/Audiovisual" ;
                         rdfs:label "Digital Audio Visual Media" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#DigitalDocument
:DigitalDocument rdf:type owl:Class ;
                 rdfs:subClassOf :DigitalMedia ;
                 d3f:definition "An digital document is any electronic media content (other than computer programs or system files) that is intended to be used in either an electronic form or as printed output." ;
                 rdfs:isDefinedBy "https://dbpedia.org/page/Electronic_document" ;
                 rdfs:label "Digital Document" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#DigitalImage
:DigitalImage rdf:type owl:Class ;
              rdfs:subClassOf :DigitalMedia ;
              d3f:definition "A digital image is a pixel-based visual representation stored in formats like JPEG or PNG, used for various digital applications." ;
              rdfs:isDefinedBy "https://dbpedia.org/page/Digital_image" ;
              rdfs:label "Digital Image" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#DigitalMedia
:DigitalMedia rdf:type owl:Class ;
              rdfs:subClassOf :StructuredDigitalInformationContent ;
              d3f:definition "Digital media refers to any communication media that operate in conjunction with various encoded machine-readable data formats." ;
              rdfs:isDefinedBy "https://dbpedia.org/page/Digital_media" ;
              rdfs:label "Digital Media" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#DigitalMultimedia
:DigitalMultimedia rdf:type owl:Class ;
                   rdfs:subClassOf :DigitalMedia ;
                   d3f:definition "Digital Multimedia refers to content that combines text, audio, images, animations, and video in a digital format for interactive applications" ;
                   rdfs:isDefinedBy "https://dbpedia.org/page/Multimedia" ;
                   rdfs:label "Digital Multimedia" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#DigitalText
:DigitalText rdf:type owl:Class ;
             rdfs:subClassOf :DigitalMedia ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty d3f:contains ;
                               owl:someValuesFrom :String
                             ] ;
             d3f:definition "Digital text is written content encoded in a digital format, allowing for storage, retrieval, and manipulation by electronic devices." ;
             rdfs:label "Digital Text" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#DigitalVideo
:DigitalVideo rdf:type owl:Class ;
              rdfs:subClassOf :DigitalMedia ;
              d3f:definition "Digital video is an electronic representation of moving visual images (video) in the form of encoded digital data." ;
              rdfs:isDefinedBy "https://dbpedia.org/page/Digital_video" ;
              rdfs:label "Digital Video" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#EncodedData
:EncodedData rdf:type owl:Class ;
             rdfs:subClassOf :StructuredDigitalInformationContent ;
             d3f:definition "Data that has been converted into a specific format." ;
             rdfs:label "Encoded Data" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#EncoderApplication
:EncoderApplication rdf:type owl:Class ;
                    rdfs:subClassOf :CodecApplication ;
                    d3f:definition "An application that encodes digital data." ;
                    rdfs:label "Encoder Application" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#Enumeration
:Enumeration rdf:type owl:Class ;
             rdfs:subClassOf :StructuredDigitalInformationContent ;
             d3f:definition "An enumerated type is a distinct type whose value is a value of its underlying type, which includes the values of explicitly named constants (enumeration constants)." ;
             rdfs:isDefinedBy "https://en.cppreference.com/w/c/language/enum" ;
             rdfs:label "Enumeration" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileContentBlock
:FileContentBlock rdf:type owl:Class ;
                  rdfs:subClassOf d3f:FileSection ;
                  d3f:definition "A section within a file that contains the main content or data payload." ;
                  rdfs:label "File Content Block" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileContentBlockData
:FileContentBlockData rdf:type owl:Class ;
                      rdfs:subClassOf :Data ;
                      d3f:definition "The actual content or main data within a file or data block." ;
                      rdfs:label "File Content Block Data" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileContentBlockMetadata
:FileContentBlockMetadata rdf:type owl:Class ;
                          rdfs:subClassOf :FileMetadata ;
                          d3f:definition "Content Blocks may contain metadata specific to the block's content at the beginning" ;
                          rdfs:label "File Content Block Metadata" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileContentDecompressionChecking
:FileContentDecompressionChecking rdf:type owl:Class ;
                                  rdfs:subClassOf :ContentValidation ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty d3f:analyzes ;
                                                    owl:someValuesFrom :FileContentBlockData
                                                  ] .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileFooterBlock
:FileFooterBlock rdf:type owl:Class ;
                 rdfs:subClassOf d3f:FileSection ;
                 d3f:definition "A section at the end of a file that contains metadata or control information." ;
                 rdfs:label "File Footer Block" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileFooterBlockContent
:FileFooterBlockContent rdf:type owl:Class ;
                        rdfs:subClassOf :FileMetadata ;
                        d3f:definition "The content of a footer block not including the signature." ;
                        rdfs:label "File Footer Block Content" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileFooterBlockSignature
:FileFooterBlockSignature rdf:type owl:Class ;
                          rdfs:subClassOf :FileMetadata ;
                          d3f:definition "A sequence of bytes used to identify and validate the footer section within a file." ;
                          rdfs:label "File Footer Block Signature" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileHeaderBlock
:FileHeaderBlock rdf:type owl:Class ;
                 rdfs:subClassOf d3f:FileSection ;
                 d3f:definition "Headers are sections of a file that organize and provide information about specific sections or components of the file. Typically found at the beginning of a file, they often contain file type identification, version information, and metadata such as size, format, and encoding." ;
                 rdfs:label "File Header Block" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileHeaderBlockContent
:FileHeaderBlockContent rdf:type owl:Class ;
                        rdfs:subClassOf :FileMetadata ;
                        d3f:definition "The content of a header block not including the signature." ;
                        rdfs:label "File Header Block Content" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileHeaderBlockSignature
:FileHeaderBlockSignature rdf:type owl:Class ;
                          rdfs:subClassOf :FileMetadata ;
                          d3f:definition "A sequence of bytes used to identify and validate specific header sections within a file." ;
                          rdfs:label "File Header Block Signature" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileInternalStructureVerification
:FileInternalStructureVerification rdf:type owl:Class ;
                                   rdfs:subClassOf :ContentValidation ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty d3f:analyzes ;
                                                     owl:someValuesFrom :FileContentBlock
                                                   ] .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileMagicByteVerification
:FileMagicByteVerification rdf:type owl:Class ;
                           rdfs:subClassOf :FileMetadataValueVerification ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty d3f:analyzes ;
                                             owl:someValuesFrom :FileMagicBytes
                                           ] .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileMagicBytes
:FileMagicBytes rdf:type owl:Class ;
                rdfs:subClassOf :FileHeaderBlockSignature ;
                d3f:definition "A specific type of header signature located at the beginning of a file, used to identify the file format." ;
                rdfs:label "File Magic Bytes" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileMetadata
:FileMetadata rdf:type owl:Class ;
              rdfs:subClassOf d3f:Metadata ;
              d3f:definition "Information that describes and provides context about a file's content, structure, and attributes." ;
              rdfs:label "File Metadata" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileMetadataConsistencyValidation
:FileMetadataConsistencyValidation rdf:type owl:Class ;
                                   rdfs:subClassOf :ContentValidation ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty d3f:analyzes ;
                                                     owl:someValuesFrom :FileContentBlockData
                                                   ] ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty d3f:analyzes ;
                                                     owl:someValuesFrom :FileMetadata
                                                   ] .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FileMetadataValueVerification
:FileMetadataValueVerification rdf:type owl:Class ;
                               rdfs:subClassOf :ContentValidation ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty d3f:analyzes ;
                                                 owl:someValuesFrom :FileFooterBlock
                                               ] ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty d3f:analyzes ;
                                                 owl:someValuesFrom :FileHeaderBlock
                                               ] .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#FloatingPoint
:FloatingPoint rdf:type owl:Class ;
               rdfs:subClassOf :UnstructuredDigitalInformationContent ;
               d3f:definition "Aarithmetic that represents real numbers approximately, using an integer with a fixed precision, called the significand, scaled by an integer exponent of a fixed base." ;
               rdfs:isDefinedBy "https://dbpedia.org/page/Floating-point_arithmetic" ;
               rdfs:label "Floating Point" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#ImageFile
:ImageFile rdf:type owl:Class ;
           rdfs:subClassOf d3f:File ;
           d3f:definition "A file that contains graphics data" ;
           rdfs:label "Image File" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#Integer
:Integer rdf:type owl:Class ;
         rdfs:subClassOf :UnstructuredDigitalInformationContent ;
         d3f:definition "Integers represents some range of mathematical integers." ;
         rdfs:isDefinedBy "https://dbpedia.org/page/Integer_(computer_science)" ;
         rdfs:label "Integer" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#MultimediaFile
:MultimediaFile rdf:type owl:Class ;
                rdfs:subClassOf d3f:File ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty d3f:contains ;
                                  owl:someValuesFrom :DigitalMultimedia
                                ] ;
                d3f:definition "A file that contains digital multimedia." ;
                rdfs:label "Multimedia File" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#NetworkAudioStreamingResource
:NetworkAudioStreamingResource rdf:type owl:Class ;
                               rdfs:subClassOf :NetworkMediaStreamingResource ;
                               d3f:definition "A server that provides digital audio media content to users." ;
                               rdfs:label "Network Audio Streaming Resource" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#NetworkAudioVisualStreamingResource
:NetworkAudioVisualStreamingResource rdf:type owl:Class ;
                                     rdfs:subClassOf :NetworkMultimediaStreamingResource ;
                                     d3f:definition "A server that provides digital audio-visual media content to users." ;
                                     rdfs:label "Network Audio Visual Streaming Resource" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#NetworkMediaStreamingResource
:NetworkMediaStreamingResource rdf:type owl:Class ;
                               rdfs:subClassOf d3f:NetworkResource ;
                               d3f:definition "A server that provides digital media content to users." ;
                               rdfs:label "Network Media Streaming Resource" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#NetworkMultimediaStreamingResource
:NetworkMultimediaStreamingResource rdf:type owl:Class ;
                                    rdfs:subClassOf :NetworkMediaStreamingResource ;
                                    d3f:definition "A server that provides digital multimedia content to users." ;
                                    rdfs:label "Network Multimedia Streaming Resource" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#NetworkVideoStreamingResource
:NetworkVideoStreamingResource rdf:type owl:Class ;
                               rdfs:subClassOf :NetworkMediaStreamingResource ;
                               d3f:definition "A server that provides digital video media content to users." ;
                               rdfs:label "Network Video Streaming Resource" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#PacketCaptureFile
:PacketCaptureFile rdf:type owl:Class ;
                   rdfs:subClassOf d3f:File ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty d3f:contains ;
                                     owl:someValuesFrom d3f:NetworkPackets
                                   ] ;
                   d3f:definition "A file which contains raw representations of collected packets." ;
                   rdfs:label "Packet Capture File" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#RTSPServer
:RTSPServer rdf:type owl:Class ;
            rdfs:subClassOf :NetworkAudioVisualStreamingResource ;
            d3f:definition "A streaming server that utilizes the real-time streaming protocol." ;
            rdfs:label "RTSP Server" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#Set
:Set rdf:type owl:Class ;
     rdfs:subClassOf :StructuredDigitalInformationContent ;
     d3f:definition "A set is an abstract data type that can store unique values, without any particular order." ;
     rdfs:isDefinedBy "https://dbpedia.org/page/Set_(abstract_data_type)" ;
     rdfs:label "Set" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#String
:String rdf:type owl:Class ;
        rdfs:subClassOf :UnstructuredDigitalInformationContent ;
        d3f:definition "A string is a sequence of characters." ;
        rdfs:isDefinedBy "https://dbpedia.org/page/String_(computer_science)" ;
        rdfs:label "String" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#StructuredDigitalInformationContent
:StructuredDigitalInformationContent rdf:type owl:Class ;
                                     rdfs:subClassOf :Data ;
                                     d3f:definition "Structured Digital Information Content refers to user data to be consumed by the user that is organized in a predefined format, making it easily searchable and analyzable. This type of content typically follows a specific schema or model, allowing for efficient data processing and retrieval." ;
                                     d3f:synonym "Complex Data Type Content" ;
                                     rdfs:isDefinedBy "https://dbpedia.org/page/Associative_array" ;
                                     rdfs:label "Structured Digital Information Content" .


###  http://d3fend.mitre.org/ontologies/d3fend-data-initiative.owl#Tuple
