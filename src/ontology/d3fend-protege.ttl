               rdfs:isDefinedBy <http://dbpedia.org/resource/HTTP_cookie#Session_cookie> ;
               rdfs:label "Session Cookie" ;
               rdfs:seeAlso <http://dbpedia.org/resource/HTTP_cookie> ;
