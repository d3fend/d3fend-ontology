            rdfs:subPropertyOf :executes ,
                               :may-interpret ;
            rdfs:comment "x interprets y: The subject x interprets the executable script y. The sense of interprets is here 'Parse the source code and perform its behavior directly.'" ;
            rdfs:label "interprets" ;
            rdfs:seeAlso <http://dbpedia.org/resource/Interpreter_(computing)> .


