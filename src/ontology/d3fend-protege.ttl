###  http://d3fend.mitre.org/ontologies/d3fend.owl#DefensiveTactic
:DefensiveTactic rdf:type owl:Class ;
                 rdfs:subClassOf :D3FENDThing ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :enabled-by ;
                                   owl:someValuesFrom :DefensiveTechnique
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :display-order ;
                                   owl:someValuesFrom xsd:integer
                                 ] ;
                 rdfs:comment "a plan for attaining a particular goal" ;
                 rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/05913746-n> ;
                 rdfs:label "Defensive Tactic" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DefensiveTechnique
:DefensiveTechnique rdf:type owl:Class ;
                    rdfs:subClassOf :CapabilityFeature ,
                                    :D3FENDThing ,
                                    :DefensiveTechnique ,
                                    :Technique ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :enables ;
                                      owl:someValuesFrom :DefensiveTactic
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :kb-reference ;
                                      owl:someValuesFrom :TechniqueReference
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :d3fend-id ;
                                      owl:someValuesFrom xsd:string
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :date ;
                                      owl:someValuesFrom xsd:dateTime
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :display-order ;
                                      owl:someValuesFrom xsd:integer
                                    ] ;
                    :definition "A method which makes a computer system more difficult to attack." ;
                    :synonym "Countermeasure Technique" ,
                             "Defensive Capability Feature" ,
                             "Technical Security Control" ;
                    rdfs:label "Defensive Technique" ;
                    rdfs:seeAlso <https://csrc.nist.gov/glossary/term/security_control> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DefensiveTechniqueAssessment
:DefensiveTechniqueAssessment rdf:type owl:Class ;
                              rdfs:subClassOf :FeatureAssessment ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :assesses ;
                                                owl:someValuesFrom :DefensiveTechniqueClaim
                                              ] ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :counters ;
                                                owl:someValuesFrom :OffensiveTechnique
                                              ] ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :confidence ;
                                                owl:someValuesFrom xsd:integer
                                              ] ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :rating ;
                                                owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                                    owl:oneOf [ rdf:type rdf:List ;
                                                                                rdf:first "0" ;
                                                                                rdf:rest [ rdf:type rdf:List ;
                                                                                           rdf:first "1" ;
                                                                                           rdf:rest [ rdf:type rdf:List ;
                                                                                                      rdf:first "2" ;
                                                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                                                 rdf:first "3" ;
                                                                                                                 rdf:rest rdf:nil
                                                                                                               ]
                                                                                                    ]
                                                                                         ]
                                                                              ]
                                                                  ]
                                              ] ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :stage ;
                                                owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                                    owl:oneOf [ rdf:type rdf:List ;
                                                                                rdf:first "Deceive" ;
                                                                                rdf:rest [ rdf:type rdf:List ;
                                                                                           rdf:first "Detect" ;
                                                                                           rdf:rest [ rdf:type rdf:List ;
                                                                                                      rdf:first "Evict" ;
                                                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                                                 rdf:first "Harden" ;
                                                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                                                            rdf:first "Isolate" ;
                                                                                                                            rdf:rest rdf:nil
                                                                                                                          ]
                                                                                                               ]
                                                                                                    ]
                                                                                         ]
                                                                              ]
                                                                  ]
                                              ] ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :rating ;
                                                owl:cardinality "1"^^xsd:nonNegativeInteger
                                              ] ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :stage ;
                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onDataRange xsd:string
                                              ] ;
                              :comment "Extend/modify this class with additional properties as necessary. Rating and stage properties demonstrate how to apply restrictions." ;
                              rdfs:comment "Assessing how well a capability implementation's capability feature functions as a countermeasure." ;
                              rdfs:label "Defensive Technique Assessment" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DefensiveTechniqueClaim
:DefensiveTechniqueClaim rdf:type owl:Class ;
                         rdfs:subClassOf :CapabilityFeatureClaim ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :cites ;
                                           owl:someValuesFrom :InformationContentEntity
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :claims ;
                                           owl:someValuesFrom :DefensiveTechnique
                                         ] ;
                         rdfs:label "Defensive Technique Claim" ;
                         <http://www.w3.org/2004/02/skos/core#altLabel> "Countermeasure Claim" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DesktopComputer
:DesktopComputer rdf:type owl:Class ;
                 rdfs:subClassOf :PersonalComputer ;
                 rdfs:comment "A desktop computer is a personal computer designed for regular use at a single location on or near a desk or table due to its size and power requirements. The most common configuration has a case that houses the power supply, motherboard (a printed circuit board with a microprocessor as the central processing unit (CPU), memory, bus, and other electronic components, disk storage (usually one or more hard disk drives, solid state drives, optical disc drives, and in early models a floppy disk drive); a keyboard and mouse for input; and a computer monitor, speakers, and, often, a printer for output. The case may be oriented horizontally or vertically and placed either underneath, beside, or on top of a desk." ;
                 rdfs:isDefinedBy <http://dbpedia.org/resource/Desktop_computer> ;
                 rdfs:label "Desktop Computer" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Detect
:Detect rdf:type owl:Class ;
        rdfs:subClassOf :DefensiveTactic .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DeveloperApplication
:DeveloperApplication rdf:type owl:Class ;
                      rdfs:subClassOf :UserApplication ;
                      rdfs:comment "An application used to develop computer software including applications used for software construction, analysis, testing, packaging, or management." ;
                      rdfs:label "Developer Application" ;
                      rdfs:seeAlso <http://dbpedia.org/resource/Application_development> ,
                                   <http://dbpedia.org/resource/Application_software> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DialUpModem
:DialUpModem rdf:type owl:Class ;
             rdfs:subClassOf :Modem ;
             rdfs:comment "A dial-up modem transmits computer data over an ordinary switched telephone line that has not been designed for data use. This contrasts with leased line modems, which also operate over lines provided by a telephone company, but ones which are intended for data use and do not impose the same signaling constraints. The modulated data must fit the frequency constraints of a normal voice audio signal, and the modem must be able to perform the actions needed to connect a call through a telephone exchange, namely: picking up the line, dialing, understanding signals sent back by phone company equipment (dial tone, ringing, busy signal,) and on the far end of the call, the second modem in the connection must be able to recognize the incoming ring signal and answer the line." ;
             rdfs:isDefinedBy <http://dbpedia.org/resource/Modem#Dial-up> ;
             rdfs:label "Dial Up Modem" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DigitalArtifact
:DigitalArtifact rdf:type owl:Class ;
                 rdfs:subClassOf :Artifact ,
                                 :DigitalObject ;
                 rdfs:comment "An information-bearing artifact (object) that is, or is encoded to be used with, a digital computer system. This concept is broad to include the literal instances of an artifact, or an implicit summarization of changes to or properties of other artifacts." ;
                 rdfs:label "Digital Artifact" ;
                 rdfs:seeAlso <http://dbpedia.org/resource/Digital_artifactual_value> ,
                              <http://dbpedia.org/resource/Virtual_artifact> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DigitalEvent
:DigitalEvent rdf:type owl:Class ;
              rdfs:subClassOf :D3FENDThing ;
              rdfs:label "Digital Event" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DigitalObject
:DigitalObject rdf:type owl:Class ;
               rdfs:subClassOf :D3FENDThing ;
               rdfs:comment "A digital object is the top-level class for an information bearing object that exists in a digital environment. The digital object may be virtual or physical." ;
               rdfs:label "Digital Object" ;
               rdfs:seeAlso <http://dbpedia.org/resource/Digital_artifactual_value> ,
                            <http://dbpedia.org/resource/Virtual_artifact> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DigitalSystem
:DigitalSystem rdf:type owl:Class ;
               rdfs:subClassOf :DigitalArtifact ;
               rdfs:comment "A digital system is a group of interacting or interrelated digital artifacts that act according to a set of rules to form a unified whole. A digital system, surrounded and influenced by its environment, is described by its boundaries, structure and purpose and expressed in its functioning. Systems are the subjects of study of systems theory." ;
               rdfs:label "Digital System" ;
               rdfs:seeAlso <http://dbpedia.org/resource/System> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Directory
:Directory rdf:type owl:Class ;
           rdfs:subClassOf :DigitalArtifact ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :may-contain ;
                             owl:someValuesFrom :File
                           ] ;
           rdfs:comment "In computing, a directory is a file system cataloging structure which contains references to other computer files, and possibly other directories. On many computers, directories are known as folders, or drawers to provide some relevancy to a workbench or the traditional office file cabinet." ;
           rdfs:isDefinedBy <http://dbpedia.org/resource/Directory_(computing)> ;
           rdfs:label "Directory" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DirectoryService
:DirectoryService rdf:type owl:Class ;
                  rdfs:subClassOf :NetworkService ;
                  rdfs:comment "In computing, directory service or name service maps the names of network resources to their respective network addresses. It is a shared information infrastructure for locating, managing, administering and organizing everyday items and network resources, which can include volumes, folders, files, printers, users, groups, devices, telephone numbers and other objects. A directory service is a critical component of a network operating system. A directory server or name server is a server which provides such a service. Each resource on the network is considered an object by the directory server. Information about a particular resource is stored as a collection of attributes associated with that resource or object." ;
                  rdfs:isDefinedBy <http://dbpedia.org/resource/Directory_service> ;
                  rdfs:label "Directory Service" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Discovery
:Discovery rdf:type owl:Class ;
           rdfs:subClassOf :OffensiveTactic .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DiscoveryTechnique
:DiscoveryTechnique rdf:type owl:Class ;
                    rdfs:subClassOf :OffensiveTechnique ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :enables ;
                                      owl:someValuesFrom :Discovery
                                    ] ;
                    rdfs:label "Discovery Technique" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DiskEncryption
:DiskEncryption rdf:type owl:Class ;
                rdfs:subClassOf :PlatformHardening ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :encrypts ;
                                  owl:someValuesFrom :Storage
                                ] .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DisplayAdapter
:DisplayAdapter rdf:type owl:Class ;
                rdfs:subClassOf :OutputDevice ;
                rdfs:comment "A graphics card (also called a display card, video card, display adapter, or graphics adapter) is an expansion card which generates a feed of output images to a display device (such as a computer monitor). Frequently, these are advertised as discrete or dedicated graphics cards, emphasizing the distinction between these and integrated graphics. At the core of both is the graphics processing unit (GPU), which is the main part that does the actual computations, but should not be confused with the video card as a whole, although \"GPU\" is often used to refer to video cards." ;
                rdfs:isDefinedBy <http://dbpedia.org/resource/Video_card> ;
                rdfs:label "Display Adapter" ;
                <http://www.w3.org/2004/02/skos/core#altLabel> "Display Card" ,
                                                               "Graphics Adapter" ,
                                                               "Video Card" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DisplayDeviceDriver
:DisplayDeviceDriver rdf:type owl:Class ;
                     rdfs:subClassOf :HardwareDriver ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :drives ;
                                       owl:someValuesFrom :DisplayAdapter
                                     ] ;
                     rdfs:comment "A device driver for a display adapter." ;
                     rdfs:label "Display Device Driver" ;
                     rdfs:seeAlso <http://dbpedia.org/resource/Device_driver> ,
                                  <http://dbpedia.org/resource/Display_adapter> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DisplayServer
:DisplayServer rdf:type owl:Class ;
               rdfs:subClassOf :DigitalArtifact ;
               rdfs:comment "A display server or window server is a program whose primary task is to coordinate the input and output of its clients to and from the rest of the operating system, the hardware, and each other. The display server communicates with its clients over the display server protocol, a communications protocol, which can be network-transparent or simply network-capable. The display server is a key component in any graphical user interface, specifically the windowing system." ;
               rdfs:isDefinedBy <http://dbpedia.org/resource/Display_server> ;
               rdfs:label "Display Server" ;
               <http://www.w3.org/2004/02/skos/core#altLabel> "Window Server" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Document
:Document rdf:type owl:Class ;
          rdfs:subClassOf :InformationContentEntity ;
          rdfs:comment "A document is a written, drawn, presented or recorded representation of thoughts." ;
          rdfs:label "Document" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DocumentFile
:DocumentFile rdf:type owl:Class ;
              rdfs:subClassOf :File ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :may-contain ;
                                owl:someValuesFrom :ExecutableScript
                              ] ;
              rdfs:comment "A document is a written, drawn, presented or recorded representation of thoughts. An electronic document file is usually used to describe a primarily textual file, along with its structure and design, such as fonts, colors and additional images." ;
              rdfs:label "Document File" ;
              rdfs:seeAlso <http://dbpedia.org/resource/Document> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DomainAccountMonitoring
:DomainAccountMonitoring rdf:type owl:Class ;
                         rdfs:subClassOf :UserBehaviorAnalysis ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :monitors ;
                                           owl:someValuesFrom :DomainUserAccount
                                         ] .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DomainName
:DomainName rdf:type owl:Class ;
            rdfs:subClassOf :Identifier ;
            rdfs:comment "A domain name is an identification string that defines a realm of administrative autonomy, authority or control within the Internet. Domain names are formed by the rules and procedures of the Domain Name System (DNS). Any name registered in the DNS is a domain name.Domain names are used in various networking contexts and application-specific naming and addressing purposes. In general, a domain name represents an Internet Protocol (IP) resource, such as a personal computer used to access the Internet, a server computer hosting a web site, or the web site itself or any other service communicated via the Internet. In 2015, 294 million domain names had been registered." ;
            rdfs:label "Domain Name" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DomainRegistration
:DomainRegistration rdf:type owl:Class ;
                    rdfs:subClassOf :DigitalArtifact ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :may-contain ;
                                      owl:someValuesFrom :DomainName
                                    ] ;
                    rdfs:comment "A domain registration, or domain name registration data, is the relevant registration data from Internet resources such as domain names, IP addresses, and autonomous system numbers. Registration data is typically retrieved by means of either the Registration Data Access Protocol (RDAP) or its predecessor, the WHOIS protocol." ;
                    rdfs:label "Domain Registration" ;
                    rdfs:seeAlso <http://dbpedia.org/resource/Domain_registration> ,
                                 <http://dbpedia.org/resource/WHOIS> ;
                    <http://www.w3.org/2004/02/skos/core#altLabel> "Domain Name Registration Data" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DomainTrustPolicy
:DomainTrustPolicy rdf:type owl:Class ;
                   rdfs:subClassOf :CredentialHardening ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :restricts ;
                                     owl:someValuesFrom :DirectoryService
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :restricts ;
                                     owl:someValuesFrom :T1087.002
                                   ] .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#DomainUserAccount
:DomainUserAccount rdf:type owl:Class ;
                   rdfs:subClassOf :UserAccount ;
                   rdfs:comment "A domain user account in Microsoft Windows (2000) defines that user's access to a logical group of network objects (computers, users, devices) that share the same Active Directory databases; that is, a user's access to a domain." ;
                   rdfs:label "Domain User Account" ;
                   rdfs:seeAlso <https://networkencyclopedia.com/global-user-account> .


