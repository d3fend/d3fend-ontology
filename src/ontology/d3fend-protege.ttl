:FileEncryption a :FileEncryption,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "File Encryption" ;
    rdfs:subClassOf :PlatformHardening,
        [ a owl:Restriction ;
            owl:onProperty :encrypts ;
