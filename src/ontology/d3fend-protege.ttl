:DatabaseQueryStringAnalysis a :DatabaseQueryStringAnalysis,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Database Query String Analysis" ;
    rdfs:subClassOf :ProcessAnalysis,
        [ a owl:Restriction ;
            owl:onProperty :analyzes ;
            owl:someValuesFrom :DatabaseQuery ] ;
