:ExecutableBinary rdf:type owl:Class ;
                  rdfs:subClassOf :ExecutableFile ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :contains ;
                                    owl:someValuesFrom :ImageCodeSegment
                                  ] ,
