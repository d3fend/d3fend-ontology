@prefix : <http://d3fend.mitre.org/ontologies/d3fend.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://d3fend.mitre.org/ontologies/d3fend.owl> .

<http://d3fend.mitre.org/ontologies/d3fend.owl> rdf:type owl:Ontology ;
                                                 <http://purl.org/dc/terms/description> "D3FEND is a framework which encodes a countermeasure knowledge base as a knowledge graph. The graph contains the types and relations that define key concepts in the cybersecurity countermeasure domain and the relations necessary to link those concepts to each other. Each of these concepts and relations are linked to references in the cybersecurity literature."^^xsd:string ;
                                                 <http://purl.org/dc/terms/license> "MIT"^^xsd:string ;
                                                 <http://purl.org/dc/terms/title> "D3FEND™ - A knowledge graph of cybersecurity countermeasures"^^xsd:string ;
                                                 rdfs:comment "Use of the D3FEND Knowledge Graph, and the associated references from this ontology are subject to the Terms of Use. D3FEND is funded by the National Security Agency (NSA) Cybersecurity Directorate and managed by the National Security Engineering Center (NSEC) which is operated by The MITRE Corporation. D3FEND™ and the D3FEND logo are trademarks of The MITRE Corporation. This software was produced for the U.S. Government under Basic Contract No. W56KGU-18-D0004, and is subject to the Rights in Noncommercial Computer Sotware and Noncommercial Computer Software Documentation Clause 252.227-7014 (FEB 2012) Copyright 2022 The MITRE Corporation."^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend.owl#attack-id
:attack-id rdf:type owl:AnnotationProperty ;
           :todo "Drop as annotation and merge any as attack-technique-id data property. Reason: The unique id of a technique may be needed even for OWL 2 DL reasoning as it is the surrogate identity key (and the primary key) for all ATT&CK techniques." ;
           rdfs:comment "x attack-id y: The offensive technique x has the att&ck unique id of y." ;
           rdfs:label "attack-id" ;
           rdfs:subPropertyOf :attack-kb-annotation ;
           rdfs:range xsd:string ;
           rdfs:domain :OffensiveTechnique .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#attack-kb-annotation
:attack-kb-annotation rdf:type owl:AnnotationProperty ;
                      rdfs:comment "x attack-kb-annotation y: The offensive technique x has the kb annotation of y." ;
                      rdfs:label "attack-kb-annotation" ;
                      rdfs:subPropertyOf :d3fend-annotation ;
                      rdfs:domain :OffensiveTechnique .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#comment
:comment rdf:type owl:AnnotationProperty ;
         rdfs:comment "x comment y: The d3fend object x a d3fend team specific comment y.  Not for use external to the MITRE development team." ;
         rdfs:label "comment" ;
         rdfs:subPropertyOf :d3fend-private-annotation ,
                            rdfs:comment .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-annotation
:d3fend-annotation rdf:type owl:AnnotationProperty ;
                   rdfs:comment "x d3fend-annotation y: The d3fend object x has the annotation y." ;
                   rdfs:label "d3fend-annotation" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-catalog-annotation-property
:d3fend-catalog-annotation-property rdf:type owl:AnnotationProperty ;
                                    rdfs:label "d3fend-catalog-annotation-property" ;
                                    <http://www.w3.org/2004/02/skos/core#altLabel> "d3fend-vendor-registry-annotation-property" ;
                                    rdfs:subPropertyOf :d3fend-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-kb-annotation-property
:d3fend-kb-annotation-property rdf:type owl:AnnotationProperty ;
                               :todo "Move all subproperties under d3fend-kb-data-property and drop this (d3fend-kb-annotation-property) property definition." ;
                               rdfs:comment "x d3fend-kb-annotation-property y: The entity x had the d3fend kb annotation y." ;
                               rdfs:label "d3fend-kb-annotation-property" ;
                               rdfs:subPropertyOf :d3fend-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-kb-reference-annotation
:d3fend-kb-reference-annotation rdf:type owl:AnnotationProperty ;
                                :todo "Move subproperties under d3fend-kb-data-property and rename this d3fend-kb-reference-property definition." ;
                                rdfs:comment "x d3fend-kb-data-property y: The reference x has the data property y." ;
                                rdfs:label "d3fend-kb-reference-annotation" ;
                                rdfs:subPropertyOf :d3fend-kb-annotation-property ;
                                rdfs:range xsd:string ;
                                rdfs:domain :Reference .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-private-annotation
:d3fend-private-annotation rdf:type owl:AnnotationProperty ;
                           rdfs:comment "x d3fend-private-annotation: The d3fend object x has the internal comment or tracking property y.  Not for use external to the MITRE development team." ;
                           rdfs:label "d3fend-private-annotation" ;
                           rdfs:subPropertyOf :d3fend-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#definition
:definition rdf:type owl:AnnotationProperty ;
            :todo "Will port rdfs:comment entries that have been definitions to d3f:definition to free up comments for non-definitional expansions and commentary." ;
            rdfs:comment "x definition y: The d3fend object x has the definition y.  Intended d3fend equivalent to http://purl.obolibrary.org/obo/IAO_0000115; will take over from use of rdfs:comment to hold definition." ;
            rdfs:isDefinedBy <http://purl.obolibrary.org/obo/IAO_0000115> ;
            rdfs:label "definition" ;
            rdfs:subPropertyOf :d3fend-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#description
:description rdf:type owl:AnnotationProperty ;
             rdfs:comment "A statement that represents something in words." ;
             rdfs:isDefinedBy "http://wordnet-rdf.princeton.edu/id/06737512-n"^^xsd:anyURI ;
             rdfs:label "description"@en ;
             rdfs:subPropertyOf :d3fend-catalog-annotation-property .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#draft
:draft rdf:type owl:AnnotationProperty ;
       rdfs:label "draft" ;
       rdfs:subPropertyOf :d3fend-private-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#kb-abstract
:kb-abstract rdf:type owl:AnnotationProperty ;
             rdfs:comment "x kb-abstract y: The reference x has the abstract y." ;
             rdfs:label "kb-abstract" ;
             rdfs:subPropertyOf :d3fend-kb-reference-annotation ;
             rdfs:range xsd:string ;
             rdfs:domain :Reference .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#kb-article
:kb-article rdf:type owl:AnnotationProperty ;
            :todo "make a d3fend-kb-data-property" ;
            rdfs:comment "The technique x has the kb-article y, where y is written in Markdown." ;
            rdfs:label "kb-article" ;
            rdfs:subPropertyOf :d3fend-kb-reference-annotation ;
            rdfs:range xsd:string ;
            rdfs:domain :Technique .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#kb-author
:kb-author rdf:type owl:AnnotationProperty ;
           :todo "Eventually would be best to resolve these to Person instances in KB.  This might be kept as kb-author-string.  Also, we might choose to drop kb- prefix on many of these." ,
                 "make a d3fend-kb-data-property" ;
           rdfs:comment "x kb-author y: The reference x has some author y." ;
           rdfs:label "kb-author" ;
           rdfs:subPropertyOf :d3fend-kb-reference-annotation ;
           rdfs:range xsd:string ;
           rdfs:domain :Reference .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#kb-mitre-analysis
:kb-mitre-analysis rdf:type owl:AnnotationProperty ;
                   :todo "make a d3fend-kb-data-property" ;
                   rdfs:comment "x kb-mitre-analysis y: The reference x has the mitre d3fend analysis y." ;
                   rdfs:label "kb-mitre-analysis" ;
                   rdfs:subPropertyOf :d3fend-kb-annotation-property ;
                   rdfs:range xsd:string ;
                   rdfs:domain :Reference .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#kb-organization
:kb-organization rdf:type owl:AnnotationProperty ;
                 :todo "Eventually would be best to resolve these to Organization instances in KB.  This might be kept as kb-organization-string.  Also, we might choose to drop kb- prefix on many of these." ,
                       "make a d3fend-kb-data-property" ;
                 rdfs:comment "x kb-organization y: The reference x was created or owned by the organization y." ;
                 rdfs:label "kb-mitre-analysis" ;
                 rdfs:subPropertyOf :d3fend-kb-annotation-property .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#label
:label rdf:type owl:AnnotationProperty ;
       rdfs:label "label"@en ;
       rdfs:subPropertyOf rdfs:label .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#pref-label
:pref-label rdf:type owl:AnnotationProperty ;
            rdfs:comment "x pref-label y: The preferred display value for x is y in d3fend tools." ;
            rdfs:label "pref-label" ;
            rdfs:subPropertyOf :d3fend-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#release-date
:release-date rdf:type owl:AnnotationProperty ;
              :todo "remove and place info in versionInfo on ontology annotations header" ;
              rdfs:comment "x release-date y: The object x has the release-date y." ;
              rdfs:label "release-date" ;
              rdfs:subPropertyOf :d3fend-annotation ,
                                 owl:versionInfo .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#synonym
:synonym rdf:type owl:AnnotationProperty ;
         rdfs:comment "an equivalent term." ;
         rdfs:label "synonym" ;
         rdfs:subPropertyOf :d3fend-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#todo
:todo rdf:type owl:AnnotationProperty ;
      rdfs:comment "x todo y: The d3fend object x has a todo annotation y specific to the d3fend team. These may serve as a guide to implementing a low-level change; before work commences should be reflected in the tasks/issues.  Not for use external to the MITRE development team." ;
      rdfs:label "todo" ;
      rdfs:subPropertyOf :d3fend-private-annotation .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty ;
                                       rdfs:label "description" .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty ;
                                   rdfs:label "license" .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty ;
                                 rdfs:label "title" .


###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdf:type owl:AnnotationProperty ;
             rdfs:label "comment" .


###  http://www.w3.org/2000/01/rdf-schema#isDefinedBy
rdfs:isDefinedBy rdf:type owl:AnnotationProperty ;
                 rdfs:label "isDefinedBy" .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#seeAlso
rdfs:seeAlso rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
<http://www.w3.org/2004/02/skos/core#altLabel> rdf:type owl:AnnotationProperty ;
                                               rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core#> ;
                                               rdfs:label "altLabel"@en .


#################################################################
#    Object Properties
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend.owl#abuses
:abuses rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :uses ;
        rdfs:comment "x abuses y: The entity x applies an artifact y to a wrong thing or person; x applies y badly or incorrectly." ;
        rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/01163606-v> ;
        rdfs:label "abuses" ;
        <http://www.w3.org/2004/02/skos/core#altLabel> "misapplies" ,
                                                       "misuses" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#accessed-by
:accessed-by rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :associated-with ,
                                :may-be-accessed-by ;
             owl:inverseOf :accesses ;
             rdfs:label "accessed-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#accesses
:accesses rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :associated-with ,
                             :may-access ;
          owl:inverseOf :associated-with ;
          rdfs:range :NetworkResource ;
          rdfs:comment "x accesses y: An subject x takes the action of reading from, writing into, or executing the stored information in the object y. Reads, writes, and executes are specific cases of accesses." ;
          rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/02673854-n> ;
          rdfs:label "accesses" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#addressed-by
:addressed-by rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :associated-with ;
              owl:inverseOf :addresses ;
              rdfs:domain :Resource ;
              rdfs:range :Identifier ;
              rdfs:comment "x addressed-by y: Relates a resource x (e.g., network host, peripheral device, disk sector, a memory cell or other logical or physical entity) to a discrete address y in an address space that points to it." ;
              rdfs:label "addressed-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#addresses
:addresses rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :associated-with ;
           rdfs:domain :Identifier ;
           rdfs:range :Resource ;
           :todo "For inference, If an object has a pointer (but is not a pointer itself), does it point to the target object of the pointer?  i.e., an image might hold a pointer pointing to some global address space, does the image itself point-to the address space?  Would we assert that directly ever or only infer it?" ;
           rdfs:comment "x addresses y: Relates a pointer x to a digital artifact y located in the address space to which x points. The address space is part of some digital store, whether it be in memory, an image, or a persistent storage device." ;
           rdfs:label "addresses" ;
           rdfs:seeAlso <http://dbpedia.org/resource/Pointer_(computer_programming)> ,
                        <http://wordnet-rdf.princeton.edu/id/02253826-v> ;
           <http://www.w3.org/2004/02/skos/core#altLabel> "points-to" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#adds
:adds rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf :associated-with ,
                         :may-add ;
      rdfs:comment "x adds y: The subject x adds a data object y, such as a file, to some other digital artifact, such as a directory. Examples include an agent or technique adding a record to a database. or a domain entry to a DNS server." ;
      rdfs:label "adds" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#analyzes
:analyzes rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :associated-with ,
                             :detects ;
          rdfs:comment "x analyzes y: The subject x break down object y into components or essential features, assessing y by quantitative methods, qualitative methods, or both.  Usually the analysis is done in terms of some model or framework." ;
          rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/00738221-v> ;
          rdfs:label "analyzes" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#assessed-by
:assessed-by rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :d3fend-catalog-object-property ;
             owl:inverseOf :assesses ;
             rdfs:label "assessed-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#assesses
:assesses rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :d3fend-catalog-object-property ;
          rdfs:domain :DefensiveTechniqueClaim ;
          rdfs:range :DefensiveTechniqueAssessment ;
          rdfs:label "assesses" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#associated-with
:associated-with rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :may-be-associated-with ;
                 rdfs:comment "x associated-with y: The subject x and object y are associated in some way.  This is the most general definite relationship in d3fend (i.e., most general relationship that is not prefixed by 'may-'.)" ;
                 rdfs:label "associated-with" ;
                 rdfs:seeAlso <http://wordnet-rdf.princeton.edu/id/13804981-n> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#attached-to
:attached-to rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :associated-with ;
             rdfs:comment "x attached-to y: A subject x is joined in close association to an object y." ;
             rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/01980375-s> ;
             rdfs:label "attached-to" ;
             rdfs:seeAlso "d3f:connects" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#attack-may-be-countered-by
:attack-may-be-countered-by rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :may-be-tactically-associated-with ;
                            rdfs:label "attack-may-be-countered-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#authenticates
:authenticates rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :associated-with ,
                                  :hardens ;
               rdfs:comment "x authenticates y: The subject x establishes the authenticity of some y. This relation indicates an authentication event has occurred." ;
               rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/01980375-s> ;
               rdfs:label "authenticates" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#author
:author rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :creator ;
        rdfs:label "author" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#authorizes
:authorizes rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :associated-with ,
                               :hardens ;
            :comment "Consider adding certificates (verb sense) specialization of authorizes for authorizations by certificate." ;
            rdfs:comment "x authorizes y: A subject x grants authorization or clearance for an agent y to use an object.  This relation indicates an authorization event has occurred." ;
            rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/00804987-v> ;
            rdfs:label "authorizes" ;
            rdfs:seeAlso <http://wordnet-rdf.princeton.edu/id/00805664-v> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#blocks
:blocks rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :counters ,
                           :filters ;
        rdfs:comment "x blocks y: The entity x blocks off the use of digital artifact y by reference to a block or allow list (or both.)" ;
        rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/01480024-v> ;
        rdfs:label "blocks" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#broader
:broader rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :semantic-relation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#broader-transitive
:broader-transitive rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :semantic-relation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#cited-by
:cited-by rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :d3fend-catalog-object-property ;
          owl:inverseOf :cites ;
          rdfs:label "cited-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#cites
:cites rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :d3fend-catalog-object-property ;
       owl:inverseOf :d3fend-catalog-object-property ;
       rdfs:range :InformationContentEntity ;
       rdfs:label "cites" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#claimed-by
:claimed-by rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :d3fend-catalog-object-property ;
            owl:inverseOf :claims ;
            rdfs:label "claimed-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#claims
:claims rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :d3fend-catalog-object-property ;
        rdfs:label "claims" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#configures
:configures rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :associated-with ,
                               :hardens ;
            rdfs:label "configures" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#connects
:connects rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :associated-with ;
          :todo "x connects y to z is a complete capture of the action, but requires and event to reify that.  y connected-to z could be relation for the state after the event \"x connects y to z\".  But there is also the likely need to model the sense of x connects y to z, where x is the communications equipment (might be a simple cable) and y and z are the systems being connected." ;
          rdfs:comment "x connects y: The subject x joins system y by means of communication equipment (to some other system, typically the adversary-targeted host)." ;
          rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/01071413-v> ;
          rdfs:label "connects" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#contained-by
:contained-by rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :associated-with ,
                                 :may-be-contained-by ;
              owl:inverseOf :contains ;
              rdf:type owl:TransitiveProperty ;
              rdfs:label "contained-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#contains
:contains rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :associated-with ,
                             :may-contain ;
          rdf:type owl:TransitiveProperty ;
          :comment """d3fend uses 'contains' for the more ontologically traditional term 'has-part' and thus is transitive.  The concept of containment in other ontologies (e.g., SUMO and OBO's RO) may refer the containment of an object within a space or object defining a space, or may express the containment of subintervals or subsequences of intervals or sequences, respectively.

Moving forward different distinctions of kinds of has-part (contains) relationships may prove useful or even necessary.  Deferred for future releases""" ;
          rdfs:comment "x contains y: A core relation that holds between a whole x and its part y.  Equivalent to relational concept 'has part' and thus transitive." ;
          rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/02639021-v> ;
          rdfs:label "contains" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#contributor
:contributor rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :d3fend-catalog-object-property ;
             :comment "See SEPIO sepio.owl for defs of subprops... still under consideration if some or all. creator, contributor, author all in schema.org as well" ,
                      "This assumes bibo's casting of dcterms:contributor as an ObjectProperty." ;
             rdfs:label "contributor" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#copies
:copies rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :creates ;
        rdfs:comment "x copies y: An technique or agent x reproduces or makes and exact copy of some digital artifact y." ;
        rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/01738810-v> ;
        rdfs:label "copies" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#counters
:counters rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :d3fend-catalog-object-property ,
                             :may-counter ;
          rdfs:label "counters" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#created-by
:created-by rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :associated-with ,
                               :may-be-created-by ;
            owl:inverseOf :creates ;
            rdfs:label "created-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#creates
:creates rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :associated-with ,
                            :may-create ;
         rdfs:comment "x creates y: The subject x bring into existence an object y.  Some technique or agent x creates a persistent digital artifact y (as opposed to production of a consumable or transient object.); i.e., bring forth or generate" ;
         rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/01630392-v> ;
         rdfs:label "creates" ;
