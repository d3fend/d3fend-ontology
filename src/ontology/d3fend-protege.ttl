:CredentialCompromiseScopeAnalysis a owl:Class,
        owl:NamedIndividual,
        :UserBehaviorAnalysis ;
    rdfs:label "Credential Compromise Scope Analysis" ;
    rdfs:subClassOf :UserBehaviorAnalysis,
        [ a owl:Restriction ;
