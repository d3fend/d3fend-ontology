           rdfs:subClassOf :Product ;
           rdfs:label "Appliance" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Application
:Application rdf:type owl:Class ;
             rdfs:subClassOf :Software ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :may-contain ;
                               owl:someValuesFrom :ApplicationConfiguration
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :uses ;
                               owl:someValuesFrom :Resource
                             ] ;
             rdfs:comment "A program that gives a computer instructions that provide the user with tools to accomplish a task; \"he has tried several different word processing applications\".  Distinct from system software that is intrinsically part of the operating system.  An application can be made up of executable files, configuration files, shared libraries, etc." ;
             rdfs:label "Application" ;
             rdfs:seeAlso <http://dbpedia.org/resource/Application_software> ,
                          <http://wordnet-rdf.princeton.edu/id/06582286-n> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationConfiguration
:ApplicationConfiguration rdf:type owl:Class ;
                          rdfs:subClassOf :ConfigurationBearingEntity ;
                          rdfs:comment "Information used to configure the parameters and initial settings for an application." ;
                          rdfs:label "Application Configuration" ;
                          rdfs:seeAlso <http://wordnet-rdf.princeton.edu/id/05739724-n> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationConfigurationDatabase
:ApplicationConfigurationDatabase rdf:type owl:Class ;
                                  rdfs:subClassOf :Database ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :contains ;
                                                    owl:someValuesFrom :ApplicationConfigurationDatabaseRecord
                                                  ] ;
                                  rdfs:comment "A database used to hold application configuration data." ;
                                  rdfs:label "Application Configuration Database" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationConfigurationDatabaseRecord
:ApplicationConfigurationDatabaseRecord rdf:type owl:Class ;
                                        rdfs:subClassOf :ApplicationConfiguration ,
                                                        :Record ;
                                        rdfs:comment "A database record holding information used to configure the parameters and initial settings for an application." ;
                                        rdfs:label "Application Configuration Database Record" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationConfigurationFile
:ApplicationConfigurationFile rdf:type owl:Class ;
                              rdfs:subClassOf :ConfigurationFile ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :contains ;
                                                owl:someValuesFrom :ApplicationConfiguration
                                              ] ;
                              rdfs:comment "A file containing Information used to configure the parameters and initial settings for an application.. A plist file is an example of this type of file for macOS.  Usually text-based." ;
                              rdfs:label "Application Configuration File" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationConfigurationHardening
:ApplicationConfigurationHardening rdf:type owl:Class ;
                                   rdfs:subClassOf :ApplicationHardening ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hardens ;
                                                     owl:someValuesFrom :ApplicationConfiguration
                                                   ] .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationHardening
:ApplicationHardening rdf:type owl:Class ;
                      rdfs:subClassOf :DefensiveTechnique ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :enables ;
                                        owl:someValuesFrom :Harden
                                      ] .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationInventorySensor
:ApplicationInventorySensor rdf:type owl:Class ;
                            rdfs:subClassOf :EndpointSensor ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :monitors ;
                                              owl:someValuesFrom :Application
                                            ] ;
                            rdfs:label "Application Inventory Sensor" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationLayerFirewall
:ApplicationLayerFirewall rdf:type owl:Class ;
                          rdfs:subClassOf :Firewall ;
                          rdfs:comment "An application firewall is a form of firewall that controls input, output, and/or access from, to, or by an application or service. It operates by monitoring and potentially blocking the input, output, or system service calls that do not meet the configured policy of the firewall. The application firewall is typically built to control all network traffic on any OSI layer up to the application layer. It is able to control applications or services specifically, unlike a stateful network firewall, which is - without additional software - unable to control network traffic regarding a specific application. There are two primary categories of application firewalls, network-based application firewalls and host-based application firewalls." ;
                          rdfs:isDefinedBy <http://dbpedia.org/resource/Application_firewall> ;
                          rdfs:label "Application Layer Firewall" ;
                          <http://www.w3.org/2004/02/skos/core#altLabel> "Application Firewall" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationProcess
:ApplicationProcess rdf:type owl:Class ;
                    rdfs:subClassOf :UserProcess ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :runs ;
                                      owl:someValuesFrom :Application
                                    ] ;
                    rdfs:comment "An application process is an instance of an application computer program that is being executed." ;
                    rdfs:label "Application Process" ;
                    rdfs:seeAlso <http://dbpedia.org/resource/Application_software> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationProcessConfiguration
:ApplicationProcessConfiguration rdf:type owl:Class ;
                                 rdfs:subClassOf :ApplicationConfiguration ;
                                 rdfs:comment "The current configuration of an application process, stored in memory. It may have been sourced from other types of application configurations, e.g. Application Configuration Files or Application Configuration Database Records." ;
                                 rdfs:label "Application Process Configuration" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationRule
:ApplicationRule rdf:type owl:Class ;
                 rdfs:subClassOf :ApplicationConfiguration ;
                 rdfs:comment "A configuration of an application which is used to apply logical or data processing functions to data processed by the application." ;
                 rdfs:label "Application Rule" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationShim
:ApplicationShim rdf:type owl:Class ;
                 rdfs:subClassOf :Shim ;
                 rdfs:comment "An application shim adapts an application program to run on a version of a platform for which they were not originally created. Most commonly \"Application Shimming\" refers to use of The Windows Application Compatibility Toolkit (ACT) provides backward compatibility by simulating the behavior of older version of Windows." ;
                 rdfs:label "Application Shim" ;
                 rdfs:seeAlso <http://dbpedia.org/resource/Shim_(computing)#Examples> ,
                              "d3f:Shim" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ArchiveFile
:ArchiveFile rdf:type owl:Class ;
             rdfs:subClassOf :File ;
             rdfs:comment "An archive file is a file that is composed of one or more computer files along with metadata. Archive files are used to collect multiple data files together into a single file for easier portability and storage, or simply to compress files to use less storage space. Archive files often store directory structures, error detection and correction information, arbitrary comments, and sometimes use built-in encryption." ;
             rdfs:isDefinedBy <http://dbpedia.org/resource/Archive_file> ;
             rdfs:label "Archive File" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Article
:Article rdf:type owl:Class ;
         rdfs:subClassOf :Document ;
         rdfs:label "Article" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Artifact
:Artifact rdf:type owl:Class ;
          rdfs:subClassOf :D3FENDThing ;
          rdfs:comment "A man-made object taken as a whole." ;
          rdfs:label "Artifact" ;
          rdfs:seeAlso <http://wordnet-rdf.princeton.edu/id/00022119-n> ,
                       <http://www.ontologyrepository.com/CommonCoreOntologies/Artifact> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ArtifactServer
:ArtifactServer rdf:type owl:Class ;
                rdfs:subClassOf :WebServer ;
                rdfs:comment "A digital artifact server provides access services to digital artifacts in a repository.  It provides an associated set of data management, search and access methods allowing application-independent access to the content." ;
                rdfs:label "Artifact Server" ;
                rdfs:seeAlso <http://dbpedia.org/resource/Content_management> ,
                             <http://dbpedia.org/resource/Content_repository> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Assessment
:Assessment rdf:type owl:Class ;
            rdfs:subClassOf :D3FENDCatalogThing ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :author ;
                              owl:someValuesFrom :Agent
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :expectation-rating ;
                              owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                  owl:oneOf [ rdf:type rdf:List ;
                                                              rdf:first "below" ;
                                                              rdf:rest [ rdf:type rdf:List ;
                                                                         rdf:first "exceeded" ;
                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                    rdf:first "met" ;
                                                                                    rdf:rest rdf:nil
                                                                                  ]
                                                                       ]
                                                            ]
                                                ]
                            ] ;
            :comment "This is really an assessment result. If we need to model the assessment as a process in the future we will append Result to these classes." ;
            rdfs:comment "The classification of someone or something with respect to its worth." ;
            rdfs:isDefinedBy <http://wordnet-rdf.princeton.edu/id/05741528-n> ;
            rdfs:label "Assessment" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#AsymmetricKey
:AsymmetricKey rdf:type owl:Class ;
               rdfs:subClassOf :CryptographicKey ;
               rdfs:comment "Asymmetric keys are public and private keys, paired such that asymmetric (public-key) cryptography algorithms can be implemented using them. Public-key cryptography, or asymmetric cryptography, is any cryptographic system that uses pairs of keys: public keys that may be disseminated widely paired with private keys which are known only to the owner. There are two functions that can be achieved: using a public key to authenticate that a message originated with a holder of the paired private key; or encrypting a message with a public key to ensure that only the holder of the paired private key can decrypt it." ;
               rdfs:label "Asymmetric Key" ;
               rdfs:seeAlso <http://dbpedia.org/resource/Public-key_cryptography> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#AudioInputDevice
:AudioInputDevice rdf:type owl:Class ;
                  rdfs:subClassOf :InputDevice ;
                  rdfs:comment "Audio input devices allow a user to send audio info to a computer for processing, recording, or carrying out commands. Devices such as microphones allow users to speak to the computer in order to record a voice message or navigate software. Aside from recording, audio input devices are also used with speech recognition software." ;
                  rdfs:isDefinedBy <http://dbpedia.org/resource/Input_device#Voice_input_devices> ;
                  rdfs:label "Audio Input Device" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#AuthenticateUser
:AuthenticateUser rdf:type owl:Class ;
                  rdfs:subClassOf :SystemCall ;
                  rdfs:label "Authenticate User" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Authentication
:Authentication rdf:type owl:Class ;
                rdfs:subClassOf :UserAction ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :authenticates ;
                                  owl:someValuesFrom :User
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :may-create ;
                                  owl:someValuesFrom :IntranetNetworkTraffic
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :originates-from ;
                                  owl:someValuesFrom :PhysicalLocation
                                ] .


