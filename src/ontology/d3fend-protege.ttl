:Client-serverPayloadProfiling a :NetworkTrafficAnalysis,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Client-server Payload Profiling" ;
    rdfs:subClassOf :NetworkTrafficAnalysis,
