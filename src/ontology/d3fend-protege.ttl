:may-be-isolated-by a owl:ObjectProperty ;
    rdfs:label "may-be-isolated-by" ;
    rdfs:subPropertyOf :attack-may-be-countered-by ;
    owl:inverseOf :may-isolate ;
    :pref-label "may be isolated by" .

