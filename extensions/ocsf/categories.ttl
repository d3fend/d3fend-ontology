@prefix d3f:     <http://d3fend.mitre.org/ontologies/d3fend.owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fun:     <http://w3id.org/sparql-generate/fn/> .
@prefix iter:    <http://w3id.org/sparql-generate/iter/> .
@prefix ocsf:    <https://schema.ocsf.io/1.3.0/ontology/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

ocsf:FindingsEventCategory
        rdf:type         owl:Class;
        rdfs:label       "Findings";
        rdfs:subClassOf  ocsf:EventCategory;
        d3f:definition   "Findings events report findings, detections, and possible resolutions of malware, anomalies, or other actions performed by security products.";
        ocsf:name        "findings" .

ocsf:IdentityAccessManagementEventCategory
        rdf:type         owl:Class;
        rdfs:label       "Identity & Access Management";
        rdfs:subClassOf  ocsf:EventCategory;
        d3f:definition   "Identity & Access Management (IAM) events relate to the supervision of the system's authentication and access control model. Examples of such events are the success or failure of authentication, granting of authority, password change, entity change, privileged use etc.";
        ocsf:name        "iam" .

ocsf:NetworkActivityEventCategory
        rdf:type         owl:Class;
        rdfs:label       "Network Activity";
        rdfs:subClassOf  ocsf:EventCategory;
        d3f:definition   "Network Activity events.";
        ocsf:name        "network" .

ocsf:DiscoveryEventCategory
        rdf:type         owl:Class;
        rdfs:label       "Discovery";
        rdfs:subClassOf  ocsf:EventCategory;
        d3f:definition   "Discovery events report the existence and state of devices, files, configurations, processes, registry keys, and other objects.";
        ocsf:name        "discovery" .

ocsf:RemediationEventCategory
        rdf:type         owl:Class;
        rdfs:label       "Remediation";
        rdfs:subClassOf  ocsf:EventCategory;
        d3f:definition   "Remediation events report the results of remediation commands targeting files, processes, and other objects.";
        ocsf:name        "remediation" .

ocsf:SystemActivityEventCategory
        rdf:type         owl:Class;
        rdfs:label       "System Activity";
        rdfs:subClassOf  ocsf:EventCategory;
        d3f:definition   "System Activity events.";
        ocsf:name        "system" .

ocsf:ApplicationActivityEventCategory
        rdf:type         owl:Class;
        rdfs:label       "Application Activity";
        rdfs:subClassOf  ocsf:EventCategory;
        d3f:definition   "Application Activity events report detailed information about the behavior of applications and services.";
        ocsf:name        "application" .
