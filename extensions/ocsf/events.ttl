@prefix d3f:     <http://d3fend.mitre.org/ontologies/d3fend.owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fun:     <http://w3id.org/sparql-generate/fn/> .
@prefix iter:    <http://w3id.org/sparql-generate/iter/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

d3f:ProcessQueryEvent
        rdf:type         owl:Class;
        rdfs:label       "Process Query Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/process_query>;
        rdfs:subClassOf  d3f:DiscoveryresultEvent;
        d3f:definition   "Process Query events report information about running processes." .

d3f:ApplicationActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Application Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/application>;
        rdfs:subClassOf  d3f:DigitalEvent .

d3f:PeripheralDeviceQueryEvent
        rdf:type         owl:Class;
        rdfs:label       "Peripheral Device Query Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/peripheral_device_query>;
        rdfs:subClassOf  d3f:DiscoveryresultEvent;
        d3f:definition   "Peripheral Device Query events report information about peripheral devices." .

d3f:ScanActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Scan Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/scan_activity>;
        rdfs:subClassOf  d3f:DigitalEvent;
        d3f:definition   "Scan events report the start, completion, and results of a scan job. The scan event includes the number of items that were scanned and the number of detections that were resolved." .

d3f:DiscoveryEvent  rdf:type  owl:Class;
        rdfs:label       "Discovery Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/discovery>;
        rdfs:subClassOf  d3f:DigitalEvent;
        d3f:definition   "The Discovery event is a generic event that defines a set of attributes available in Discovery category events. As a generic event, it could be used to log events that are not otherwise defined by the Discovery specific event classes." .

d3f:ModuleActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Module Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/module_activity>;
        rdfs:subClassOf  d3f:SystemEvent;
        d3f:definition   "Module  Activity events report when a process loads or unloads the <code>module</code>." .

d3f:DatastoreActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Datastore Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/datastore_activity>;
        rdfs:subClassOf  d3f:ApplicationEvent;
        d3f:definition   "Datastore events describe general activities (Read, Update, Query, Delete, etc.) which affect datastores or data within those datastores, e.g. (AWS RDS, AWS S3)." .

d3f:KernelActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Kernel Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/kernel_activity>;
        rdfs:subClassOf  d3f:SystemEvent;
        d3f:definition   "Kernel Activity events report when an process creates, reads, or deletes a kernel resource." .

d3f:FileQueryEvent  rdf:type  owl:Class;
        rdfs:label       "File Query Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/file_query>;
        rdfs:subClassOf  d3f:DiscoveryresultEvent;
        d3f:definition   "File Query events report information about files that are present on the system." .

d3f:JobQueryEvent  rdf:type  owl:Class;
        rdfs:label       "Job Query Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/job_query>;
        rdfs:subClassOf  d3f:DiscoveryresultEvent;
        d3f:definition   "Job Query events report information about scheduled jobs." .

d3f:KernelExtensionActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Kernel Extension Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/kernel_extension>;
        rdfs:subClassOf  d3f:SystemEvent;
        d3f:definition   "Kernel Extension events report when a driver/extension is loaded or unloaded into the kernel" .

d3f:OperatingSystemPatchStateEvent
        rdf:type         owl:Class;
        rdfs:label       "Operating System Patch State Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/patch_state>;
        rdfs:subClassOf  d3f:DiscoveryEvent;
        d3f:definition   "Operating System Patch State reports the installation of an OS patch to a device and any associated knowledgebase articles." .

d3f:ApplicationLifecycleEvent
        rdf:type         owl:Class;
        rdfs:label       "Application Lifecycle Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/application_lifecycle>;
        rdfs:subClassOf  d3f:ApplicationEvent;
        d3f:definition   "Application Lifecycle events report installation, removal, start, stop of an application or service." .

d3f:IncidentFindingEvent
        rdf:type         owl:Class;
        rdfs:label       "Incident Finding Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/incident_finding>;
        rdfs:subClassOf  d3f:DigitalEvent;
        d3f:definition   "An Incident Finding reports the creation, update, or closure of security incidents as a result of detections and/or analytics." .

d3f:DataSecurityFindingEvent
        rdf:type         owl:Class;
        rdfs:label       "Data Security Finding Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/data_security_finding>;
        rdfs:subClassOf  d3f:FindingEvent;
        d3f:definition   "A Data Security Finding describes detections or alerts generated by various data security products such as Data Loss Prevention (DLP), Data Classification, Secrets Management, Digital Rights Management (DRM), Data Security Posture Management (DSPM), and similar tools. These detections or alerts can be created using fingerprinting, statistical analysis, machine learning or other methodologies. The finding describes the actors and endpoints who accessed or own the sensitive data, as well as the resources which store the sensitive data." .

d3f:FindingEvent  rdf:type  owl:Class;
        rdfs:label       "Finding Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/finding>;
        rdfs:subClassOf  d3f:DigitalEvent;
        d3f:definition   "The Finding event is a generic event that defines a set of attributes available in the Findings category." .

d3f:EmailFileActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Email File Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/email_file_activity>;
        rdfs:subClassOf  d3f:DigitalEvent;
        d3f:definition   "Email File Activity events report files within emails." .

d3f:VulnerabilityFindingEvent
        rdf:type         owl:Class;
        rdfs:label       "Vulnerability Finding Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/vulnerability_finding>;
        rdfs:subClassOf  d3f:FindingEvent;
        d3f:definition   "The Vulnerability Finding event is a notification about weakness in an information system, system security procedures, internal controls, or implementation that could be exploited or triggered by a threat source." .

d3f:ComplianceFindingEvent
        rdf:type         owl:Class;
        rdfs:label       "Compliance Finding Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/compliance_finding>;
        rdfs:subClassOf  d3f:FindingEvent;
        d3f:definition   "Compliance Finding events describe results of evaluations performed against resources, to check compliance with various Industry Frameworks or Security Standards such as <code>NIST SP 800-53, CIS AWS Foundations Benchmark v1.4.0, ISO/IEC 27001</code> etc." .

d3f:IdentityAccessManagementEvent
        rdf:type         owl:Class;
        rdfs:label       "Identity & Access Management Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/iam>;
        rdfs:subClassOf  d3f:DigitalEvent;
        d3f:definition   "The Identity & Access Management event is a generic event that defines a set of attributes available in the access control events. As a generic event, it could be used to log events that are not otherwise defined by the IAM category." .

d3f:DeviceConfigStateEvent
        rdf:type         owl:Class;
        rdfs:label       "Device Config State Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/config_state>;
        rdfs:subClassOf  d3f:DiscoveryEvent;
        d3f:definition   "Device Config State events report device configuration data and CIS Benchmark results." .

d3f:DHCPActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "DHCP Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/dhcp_activity>;
        rdfs:subClassOf  d3f:NetworkEvent;
        d3f:definition   "DHCP Activity events report MAC to IP assignment via DHCP from a client or server." .

d3f:NetworkEvent  rdf:type  owl:Class;
        rdfs:label       "Network Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/network>;
        rdfs:subClassOf  d3f:DigitalEvent;
        d3f:definition   "Network event is a generic event that defines a set of attributes available in the Network category." .

d3f:HTTPActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "HTTP Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/http_activity>;
        rdfs:subClassOf  d3f:NetworkEvent;
        d3f:definition   "HTTP Activity events report HTTP connection and traffic information." .

d3f:FileHostingActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "File Hosting Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/file_hosting>;
        rdfs:subClassOf  d3f:ApplicationEvent;
        d3f:definition   "File Hosting Activity events report the actions taken by file management applications, including file sharing servers like Sharepoint and services such as Box, MS OneDrive, or Google Drive." .

d3f:ServiceQueryEvent
        rdf:type         owl:Class;
        rdfs:label       "Service Query Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/service_query>;
        rdfs:subClassOf  d3f:DiscoveryresultEvent;
        d3f:definition   "Service Query events report information about running services." .

d3f:AdminGroupQueryEvent
        rdf:type         owl:Class;
        rdfs:label       "Admin Group Query Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/admin_group_query>;
        rdfs:subClassOf  d3f:DiscoveryresultEvent;
        d3f:definition   "Admin Group Query events report information about administrative groups." .

d3f:EntityManagementEvent
        rdf:type         owl:Class;
        rdfs:label       "Entity Management Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/entity_management>;
        rdfs:subClassOf  d3f:IamEvent;
        d3f:definition   "Entity Management events report activity by a managed client, a micro service, or a user at a management console. The activity can be a create, read, update, and delete operation on a managed entity." .

d3f:WebResourceAccessActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Web Resource Access Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/web_resource_access_activity>;
        rdfs:subClassOf  d3f:ApplicationEvent;
        d3f:definition   "Web Resource Access Activity events describe successful/failed attempts to access a web resource over HTTP." .

d3f:UserQueryEvent  rdf:type  owl:Class;
        rdfs:label       "User Query Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/user_query>;
        rdfs:subClassOf  d3f:DiscoveryresultEvent;
        d3f:definition   "User Query events report user data that have been discovered, queried, polled or searched. This event differs from User Inventory as it describes the result of a targeted search by filtering a subset of user attributes." .

d3f:SMBActivityEvent  rdf:type  owl:Class;
        rdfs:label       "SMB Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/smb_activity>;
        rdfs:subClassOf  d3f:NetworkEvent;
        d3f:definition   "Server Message Block (SMB) Protocol Activity events report client/server connections sharing resources within the network." .

d3f:AuthorizeSessionEvent
        rdf:type         owl:Class;
        rdfs:label       "Authorize Session Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/authorize_session>;
        rdfs:subClassOf  d3f:IamEvent;
        d3f:definition   "Authorize Session events report privileges or groups assigned to a new user session, usually at login time." .

d3f:ScheduledJobActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Scheduled Job Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/scheduled_job_activity>;
        rdfs:subClassOf  d3f:SystemEvent;
        d3f:definition   "Scheduled Job Activity events report activities related to scheduled jobs or tasks." .

d3f:APIActivityEvent  rdf:type  owl:Class;
        rdfs:label       "API Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/api_activity>;
        rdfs:subClassOf  d3f:ApplicationEvent;
        d3f:definition   "API events describe general CRUD (Create, Read, Update, Delete) API activities, e.g. (AWS Cloudtrail)" .

d3f:MemoryActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Memory Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/memory_activity>;
        rdfs:subClassOf  d3f:SystemEvent;
        d3f:definition   "Memory Activity events report when a process has memory allocated, read/modified, or other manipulation activities - such as a buffer overflow or turning off data execution protection (DEP)." .

d3f:ProcessActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Process Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/process_activity>;
        rdfs:subClassOf  d3f:SystemEvent;
        d3f:definition   "Process Activity events report when a process launches, injects, opens or terminates another process, successful or otherwise." .

d3f:KernelObjectQueryEvent
        rdf:type         owl:Class;
        rdfs:label       "Kernel Object Query Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/kernel_object_query>;
        rdfs:subClassOf  d3f:DiscoveryresultEvent;
        d3f:definition   "Kernel Object Query events report information about discovered kernel resources." .

d3f:NetworkConnectionQueryEvent
        rdf:type         owl:Class;
        rdfs:label       "Network Connection Query Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/network_connection_query>;
        rdfs:subClassOf  d3f:DiscoveryresultEvent;
        d3f:definition   "Network Connection Query events report information about active network connections." .

d3f:TunnelActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Tunnel Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/tunnel_activity>;
        rdfs:subClassOf  d3f:NetworkEvent;
        d3f:definition   "Tunnel Activity events report secure tunnel establishment (such as VPN), teardowns, renewals, and other network tunnel specific actions." .

d3f:NTPActivityEvent  rdf:type  owl:Class;
        rdfs:label       "NTP Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/ntp_activity>;
        rdfs:subClassOf  d3f:NetworkEvent;
        d3f:definition   "The Network Time Protocol (NTP) Activity events report instances of remote clients synchronizing their clocks with an NTP server, as observed on the network." .

d3f:EmailActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Email Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/email_activity>;
        rdfs:subClassOf  d3f:DigitalEvent;
        d3f:definition   "Email events report activities of emails." .

d3f:DeviceConfigStateChangeEvent
        rdf:type         owl:Class;
        rdfs:label       "Device Config State Change Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/device_config_state_change>;
        rdfs:subClassOf  d3f:DiscoveryEvent;
        d3f:definition   "Device Config State Change events report state changes that impact the security of the device." .

d3f:FTPActivityEvent  rdf:type  owl:Class;
        rdfs:label       "FTP Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/ftp_activity>;
        rdfs:subClassOf  d3f:NetworkEvent;
        d3f:definition   "File Transfer Protocol (FTP) Activity events report file transfers between a server and a client as seen on the network." .

d3f:FileSystemActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "File System Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/file_activity>;
        rdfs:subClassOf  d3f:SystemEvent;
        d3f:definition   "File System Activity events report when a process performs an action on a file or folder." .

d3f:NetworkRemediationActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Network Remediation Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/network_remediation_activity>;
        rdfs:subClassOf  d3f:RemediationactivityEvent;
        d3f:definition   "Network Remediation Activity events report on attempts at remediating computer networks. It follows the MITRE countermeasures defined by the D3FEND™ <a target='_blank' href='https://d3fend.mitre.org/'>Matrix</a>. Techniques and Sub-techniques will include Network, such as Network Isolation or Network Traffic Filtering." .

d3f:FolderQueryEvent  rdf:type  owl:Class;
        rdfs:label       "Folder Query Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/folder_query>;
        rdfs:subClassOf  d3f:DiscoveryresultEvent;
        d3f:definition   "Folder Query events report information about folders that are present on the system." .

d3f:ProcessRemediationActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Process Remediation Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/process_remediation_activity>;
        rdfs:subClassOf  d3f:RemediationactivityEvent;
        d3f:definition   "Process Remediation Activity events report on attempts at remediating processes. It follows the MITRE countermeasures defined by the D3FEND™ <a target='_blank' href='https://d3fend.mitre.org/'>Matrix</a>. Sub-techniques will include Process, such as Process Termination or Kernel-based Process Isolation." .

d3f:DNSActivityEvent  rdf:type  owl:Class;
        rdfs:label       "DNS Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/dns_activity>;
        rdfs:subClassOf  d3f:NetworkEvent;
        d3f:definition   "DNS Activity events report DNS queries and answers as seen on the network." .

d3f:FileRemediationActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "File Remediation Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/file_remediation_activity>;
        rdfs:subClassOf  d3f:RemediationactivityEvent;
        d3f:definition   "File Remediation Activity events report on attempts at remediating files. It follows the MITRE countermeasures defined by the D3FEND™ <a target='_blank' href='https://d3fend.mitre.org/'>Matrix</a>. Sub-techniques will include File, such as File Removal or Restore File." .

d3f:GroupManagementEvent
        rdf:type         owl:Class;
        rdfs:label       "Group Management Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/group_management>;
        rdfs:subClassOf  d3f:IamEvent;
        d3f:definition   "Group Management events report management updates to a group, including updates to membership and permissions." .

d3f:RDPActivityEvent  rdf:type  owl:Class;
        rdfs:label       "RDP Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/rdp_activity>;
        rdfs:subClassOf  d3f:NetworkEvent;
        d3f:definition   "Remote Desktop Protocol (RDP) Activity events report remote client connections to a server as seen on the network." .

d3f:SSHActivityEvent  rdf:type  owl:Class;
        rdfs:label       "SSH Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/ssh_activity>;
        rdfs:subClassOf  d3f:NetworkEvent;
        d3f:definition   "SSH Activity events report remote client connections to a server using the Secure Shell (SSH) Protocol." .

d3f:UserInventoryInfoEvent
        rdf:type         owl:Class;
        rdfs:label       "User Inventory Info Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/user_inventory>;
        rdfs:subClassOf  d3f:DiscoveryEvent;
        d3f:definition   "User Inventory Info events report user inventory data that is either logged or proactively collected. For example, when collecting user information from Active Directory entries." .

d3f:DetectionFindingEvent
        rdf:type         owl:Class;
        rdfs:label       "Detection Finding Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/detection_finding>;
        rdfs:subClassOf  d3f:FindingEvent;
        d3f:definition   "A Detection Finding describes detections or alerts generated by security products using correlation engines, detection engines or other methodologies. Note: if the product is a security control, the <code>security_control</code> profile should be applied and its <code>attacks</code> information should be duplicated into the <code>finding_info</code> object." .

d3f:DiscoveryResultEvent
        rdf:type         owl:Class;
        rdfs:label       "Discovery Result Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/discovery_result>;
        rdfs:subClassOf  d3f:DigitalEvent;
        d3f:definition   "Discovery Result events report the results of a discovery request." .

d3f:SecurityFindingEvent
        rdf:type         owl:Class;
        rdfs:label       "Security Finding Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/security_finding>;
        rdfs:subClassOf  d3f:DigitalEvent;
        d3f:definition   "Security Finding events describe findings, detections, anomalies, alerts and/or actions performed by security products" .

d3f:NetworkActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Network Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/network_activity>;
        rdfs:subClassOf  d3f:NetworkEvent;
        d3f:definition   "Network Activity events report network connection and traffic activity." .

d3f:WebResourcesActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Web Resources Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/web_resources_activity>;
        rdfs:subClassOf  d3f:DigitalEvent;
        d3f:definition   "Web Resources Activity events describe actions executed on a set of Web Resources." .

d3f:DeviceInventoryInfoEvent
        rdf:type         owl:Class;
        rdfs:label       "Device Inventory Info Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/inventory_info>;
        rdfs:subClassOf  d3f:DiscoveryEvent;
        d3f:definition   "Device Inventory Info events report device inventory data that is either logged or proactively collected. For example, when collecting device information from a CMDB or running a network sweep of connected devices." .

d3f:RemediationActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Remediation Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/remediation_activity>;
        rdfs:subClassOf  d3f:DigitalEvent;
        d3f:definition   "Remediation Activity events report on attempts at remediating a compromised device or computer network. It follows the MITRE countermeasures defined by the D3FEND™ <a target='_blank' href='https://d3fend.mitre.org/'>Matrix</a>." .

d3f:EventLogActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Event Log Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/event_log>;
        rdfs:subClassOf  d3f:SystemEvent;
        d3f:definition   "Event Log Activity events report actions pertaining to the system's event logging service(s), such as disabling logging or clearing the log data." .

d3f:SoftwareInventoryInfoEvent
        rdf:type         owl:Class;
        rdfs:label       "Software Inventory Info Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/software_info>;
        rdfs:subClassOf  d3f:DiscoveryEvent;
        d3f:definition   "Software Inventory Info events report device software inventory data that is either logged or proactively collected. For example, when collecting device information from a CMDB or running a network sweep of connected devices." .

d3f:NetworksQueryEvent
        rdf:type         owl:Class;
        rdfs:label       "Networks Query Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/networks_query>;
        rdfs:subClassOf  d3f:DiscoveryresultEvent;
        d3f:definition   "Networks Query events report information about network adapters." .

d3f:BaseEventEvent  rdf:type  owl:Class;
        rdfs:label      "Base Event Event";
        rdfs:seeAlso    <https://schema.ocsf.io/1.3.0/classes/base_event>;
        d3f:definition  "The base event is a generic and concrete event. It also defines a set of attributes available in most event classes. As a generic event that does not belong to any event category, it could be used to log events that are not otherwise defined by the schema." .

d3f:EmailURLActivityEvent
        rdf:type         owl:Class;
        rdfs:label       "Email URL Activity Event";
        rdfs:seeAlso     <https://schema.ocsf.io/1.3.0/classes/email_url_activity>;
        rdfs:subClassOf  d3f:DigitalEvent;
