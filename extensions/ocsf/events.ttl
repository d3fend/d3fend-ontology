ocsf:FindingEvent  rdf:type  owl:Class;
        rdfs:label           "Finding";
        rdfs:subClassOf      "base_event";
        rdfs:subClassOf      d3f:DigitalEvent;
        dcterms:description  "The Finding event is a generic event that defines a set of attributes available in the Findings category.";
        ocsf:attributes      "{\"activity_name\":{\"description\":\"The finding activity name, as defined by the \\u003ccode\\u003eactivity_id\\u003c/code\\u003e.\"},\"activity_id\":{\"description\":\"The normalized identifier of the finding activity.\",\"enum\":{\"1\":{\"caption\":\"Create\",\"description\":\"A finding was created.\"},\"2\":{\"caption\":\"Update\",\"description\":\"A finding was updated.\"},\"3\":{\"caption\":\"Close\",\"description\":\"A finding was closed.\"}}},\"comment\":{\"description\":\"A user provided comment about the finding.\",\"group\":\"context\",\"requirement\":\"optional\"},\"confidence\":{\"group\":\"context\",\"requirement\":\"optional\"},\"confidence_id\":{\"group\":\"context\",\"requirement\":\"recommended\"},\"confidence_score\":{\"group\":\"context\",\"requirement\":\"optional\"},\"end_time\":{\"description\":\"The time of the most recent event included in the finding.\",\"requirement\":\"optional\"},\"finding_info\":{\"group\":\"primary\",\"requirement\":\"required\"},\"start_time\":{\"description\":\"The time of the least recent event included in the finding.\",\"requirement\":\"optional\"},\"status\":{\"description\":\"The normalized status of the Finding set by the consumer normalized to the caption of the status_id value. In the case of \\u0027Other\\u0027, it is defined by the source.\",\"group\":\"context\",\"requirement\":\"optional\"},\"status_id\":{\"description\":\"The normalized status identifier of the Finding, set by the consumer.\",\"group\":\"context\",\"requirement\":\"recommended\",\"enum\":{\"1\":{\"caption\":\"New\",\"description\":\"The Finding is new and yet to be reviewed.\"},\"2\":{\"caption\":\"In Progress\",\"description\":\"The Finding is under review.\"},\"3\":{\"caption\":\"Suppressed\",\"description\":\"The Finding was reviewed, determined to be benign or a false positive and is now suppressed.\"},\"4\":{\"caption\":\"Resolved\",\"description\":\"The Finding was reviewed, remediated and is now considered resolved.\"}}}}";
        ocsf:category        "findings";
        ocsf:name            "finding" .

ocsf:SecurityFindingEvent
        rdf:type             owl:Class;
        rdfs:label           "Security Finding";
        rdfs:subClassOf      "base_event";
        rdfs:subClassOf      d3f:DigitalEvent;
        dcterms:description  "Security Finding events describe findings, detections, anomalies, alerts and/or actions performed by security products";
