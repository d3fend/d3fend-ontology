        ocsf:attributes      "{\"build\":{\"requirement\":\"optional\"},\"country\":{\"description\":\"The operating system country code, as defined by the ISO 3166-1 standard (Alpha-2 code). For the complete list of country codes, see \\u003ca target\\u003d\\u0027_blank\\u0027 href\\u003d\\u0027https://www.iso.org/obp/ui/#iso:pub:PUB500001:en\\u0027\\u003eISO 3166-1 alpha-2 codes\\u003c/a\\u003e.\",\"requirement\":\"optional\"},\"cpe_name\":{\"requirement\":\"optional\"},\"cpu_bits\":{\"requirement\":\"optional\"},\"edition\":{\"requirement\":\"optional\"},\"lang\":{\"requirement\":\"optional\"},\"name\":{\"description\":\"The operating system name.\",\"requirement\":\"required\"},\"sp_name\":{\"requirement\":\"optional\"},\"sp_ver\":{\"requirement\":\"optional\"},\"type\":{\"description\":\"The type of the operating system.\",\"requirement\":\"optional\"},\"type_id\":{\"description\":\"The type identifier of the operating system.\",\"requirement\":\"required\",\"enum\":{\"100\":{\"caption\":\"Windows\"},\"101\":{\"caption\":\"Windows Mobile\"},\"200\":{\"caption\":\"Linux\"},\"201\":{\"caption\":\"Android\"},\"300\":{\"caption\":\"macOS\"},\"301\":{\"caption\":\"iOS\"},\"302\":{\"caption\":\"iPadOS\"},\"400\":{\"caption\":\"Solaris\"},\"401\":{\"caption\":\"AIX\"},\"402\":{\"caption\":\"HP-UX\"}}},\"version\":{\"description\":\"The version of the OS running on the device that originated the event. For example: \\\"Windows 10\\\", \\\"OS X 10.7\\\", or \\\"iOS 9\\\".\",\"requirement\":\"optional\"}}";
