@prefix d3f:        <http://d3fend.mitre.org/ontologies/d3fend.owl#> .
@prefix dcterms:    <http://purl.org/dc/terms/> .
@prefix fun:        <http://w3id.org/sparql-generate/fn/> .
@prefix iter:       <http://w3id.org/sparql-generate/iter/> .
@prefix jsonschema: <https://www.w3.org/2019/wot/json-schema#> .
@prefix ocsf:       <https://schema.ocsf.io/1.1.0-dev/ontology/> .
@prefix owl:        <http://www.w3.org/2002/07/owl#> .
@prefix rdf:        <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:       <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:        <http://www.w3.org/2001/XMLSchema#> .

ocsf:CVSSScore  rdf:type     owl:Class;
        rdfs:label           "CVSS Score";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:metrics;
                               owl:someValuesFrom  ocsf:Metric
                             ];
        dcterms:description  "The Common Vulnerability Scoring System (<a target='_blank' href='https://www.first.org/cvss/'>CVSS</a>) object provides a way to capture the principal characteristics of a vulnerability and produce a numerical score reflecting its severity.";
        ocsf:name            "cvss" .

ocsf:Remediation  rdf:type   owl:Class;
        rdfs:label           "Remediation";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:kb_articles;
                               owl:someValuesFrom  ocsf:KBArticle
                             ];
        dcterms:description  "The Remediation object describes the recommended remediation steps to address identified issue(s).";
        ocsf:name            "remediation" .

ocsf:Finding  rdf:type       owl:Class;
        rdfs:label           "Finding";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:remediation;
                               owl:someValuesFrom  ocsf:Remediation
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:related_events;
                               owl:someValuesFrom  ocsf:RelatedEvent
                             ];
        dcterms:description  "The Finding object describes metadata related to a security finding generated by a security tool or system.";
        ocsf:name            "finding" .

ocsf:RelatedEvent  rdf:type  owl:Class;
        rdfs:label           "Related Event";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:observables;
                               owl:someValuesFrom  ocsf:Observable
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:kill_chain;
                               owl:someValuesFrom  ocsf:KillChainPhase
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:attacks;
                               owl:someValuesFrom  ocsf:MITREATTCK
                             ];
        dcterms:description  "The Related Event object describes an event related to a finding or detection as identified by the security product.";
        ocsf:name            "related_event" .

ocsf:HASSH  rdf:type         owl:Class;
        rdfs:label           "HASSH";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type         owl:Restriction;
                               owl:cardinality  1;
                               owl:onProperty   ocsf:fingerprint
                             ];
        dcterms:description  "The HASSH object contains SSH network fingerprinting values for specific client/server implementations. It provides a standardized way of identifying and categorizing SSH connections based on their unique characteristics and behavior.";
        ocsf:name            "hassh" .

ocsf:Process  rdf:type       owl:Class;
        rdfs:label           "Process";
        rdfs:subClassOf      ocsf:Entity;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:parent_process;
                               owl:someValuesFrom  ocsf:Process
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:xattributes;
                               owl:someValuesFrom  ocsf:Object
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:session;
                               owl:someValuesFrom  ocsf:Session
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:file;
                               owl:someValuesFrom  ocsf:File
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:user;
                               owl:someValuesFrom  ocsf:User
                             ];
        dcterms:description  "The Process object describes a running instance of a launched program. Defined by D3FEND <a target='_blank' href='https://d3fend.mitre.org/dao/artifact/d3f:Process/'>d3f:Process</a>.";
        ocsf:constraints     "{\"at_least_one\":[\"pid\",\"uid\"]}";
        ocsf:name            "process";
        ocsf:observable      25;
        ocsf:profiles        "[\"container\"]" .

ocsf:NetworkTraffic  rdf:type  owl:Class;
        rdfs:label           "Network Traffic";
        rdfs:subClassOf      ocsf:Object;
        dcterms:description  "The Network Traffic object describes characteristics of network traffic. Network traffic refers to data moving across a network at a given point of time. Defined by D3FEND <a target='_blank' href='https://d3fend.mitre.org/dao/artifact/d3f:NetworkTraffic/'>d3f:NetworkTraffic</a>.";
        ocsf:name            "network_traffic" .

ocsf:Organization  rdf:type  owl:Class;
        rdfs:label           "Organization";
        rdfs:subClassOf      ocsf:Entity;
        dcterms:description  "The Organization object describes characteristics of an orgnization or company and its division if any.";
        ocsf:name            "organization" .

ocsf:Compliance  rdf:type    owl:Class;
        rdfs:label           "Compliance";
        rdfs:subClassOf      ocsf:Object;
        dcterms:description  "The Compliance object contains information about Industry and Regulatory Framework standards, controls and requirements.";
        ocsf:name            "compliance" .

ocsf:PeripheralDevice
        rdf:type             owl:Class;
        rdfs:label           "Peripheral Device";
        rdfs:subClassOf      ocsf:Entity;
        dcterms:description  "The peripheral device object describes the identity, vendor and model of a peripheral device.";
        ocsf:name            "peripheral_device" .

ocsf:DigitalCertificate
        rdf:type             owl:Class;
        rdfs:label           "Digital Certificate";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type         owl:Restriction;
                               owl:cardinality  1;
                               owl:onProperty   ocsf:fingerprints
                             ];
        dcterms:description  "The Digital Certificate, also known as a Public Key Certificate, object contains information about the ownership and usage of a public key. It serves as a means to establish trust in the authenticity and integrity of the public key and the associated entity. Defined by D3FEND <a target='_blank' href='https://d3fend.mitre.org/dao/artifact/d3f:Certificate/'>d3f:Certificate</a>.";
        ocsf:name            "certificate" .

ocsf:KernelResource  rdf:type  owl:Class;
        rdfs:label           "Kernel Resource";
        rdfs:subClassOf      ocsf:Object;
        dcterms:description  "The Kernel Resource object provides information about a specific kernel resource, including its name and type. It describes essential attributes associated with a resource managed by the kernel of an operating system. Defined by D3FEND <a target='_blank' href='https://d3fend.mitre.org/dao/artifact/d3f:Kernel/'>d3f:Kernel</a>.";
        ocsf:name            "kernel" .

ocsf:File  rdf:type          owl:Class;
        rdfs:label           "File";
        rdfs:subClassOf      ocsf:Entity;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:signature;
                               owl:someValuesFrom  ocsf:DigitalSignature
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:xattributes;
                               owl:someValuesFrom  ocsf:Object
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:product;
                               owl:someValuesFrom  ocsf:Product
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:owner;
                               owl:someValuesFrom  ocsf:User
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:hashes;
                               owl:someValuesFrom  ocsf:Fingerprint
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:creator;
                               owl:someValuesFrom  ocsf:User
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:accessor;
                               owl:someValuesFrom  ocsf:User
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:modifier;
                               owl:someValuesFrom  ocsf:User
                             ];
        dcterms:description  "The File object represents the metadata associated with a file stored in a computer system. It encompasses information about the file itself, including its attributes, properties, and organizational details. Defined by D3FEND <a target='_blank' href='https://next.d3fend.mitre.org/dao/artifact/d3f:File/'>d3f:File</a>.";
        ocsf:constraints     "{}";
        ocsf:name            "file";
        ocsf:observable      24 .

ocsf:CISBenchmarkResult
        rdf:type             owl:Class;
        rdfs:label           "CIS Benchmark Result";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:rule;
                               owl:someValuesFrom  ocsf:Rule
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:remediation;
                               owl:someValuesFrom  ocsf:Remediation
                             ];
        dcterms:description  "The CIS Benchmark Result object contains information as defined by the Center for Internet Security (<a target='_blank' href='https://www.cisecurity.org/cis-benchmarks/'>CIS</a>) benchmark result. CIS Benchmarks are a collection of best practices for securely configuring IT systems, software, networks, and cloud infrastructure.";
        ocsf:name            "cis_benchmark_result" .

ocsf:CISControl  rdf:type    owl:Class;
        rdfs:label           "CIS Control";
        rdfs:subClassOf      ocsf:Object;
        dcterms:description  "The CIS Control (aka Critical Security Control) object describes a prioritized set of actions to protect your organization and data from cyber-attack vectors. The <a target='_blank' href='https://www.cisecurity.org/controls'>CIS Controls</a> are defined by the Center for Internet Security.";
        ocsf:name            "cis_control" .

ocsf:VulnerabilityDetails
        rdf:type             owl:Class;
        rdfs:label           "Vulnerability Details";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:affected_code;
                               owl:someValuesFrom  ocsf:AffectedCode
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:remediation;
                               owl:someValuesFrom  ocsf:Remediation
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:packages;
                               owl:someValuesFrom  ocsf:SoftwarePackage
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:affected_packages;
                               owl:someValuesFrom  ocsf:AffectedSoftwarePackage
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:cwe;
                               owl:someValuesFrom  ocsf:CWE
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:cve;
                               owl:someValuesFrom  ocsf:CVE
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:kb_articles;
                               owl:someValuesFrom  ocsf:KBArticle
                             ];
        dcterms:description  "The vulnerability is an unintended characteristic of a computing component or system configuration that multiplies the risk of an adverse event or a loss occurring either due to accidental exposure, deliberate attack, or conflict with new system components.";
        ocsf:constraints     "{\"at_least_one\":[\"cve\",\"cwe\"]}";
        ocsf:name            "vulnerability" .

ocsf:DeviceHardwareInfo
        rdf:type             owl:Class;
        rdfs:label           "Device Hardware Info";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:keyboard_info;
                               owl:someValuesFrom  ocsf:KeyboardInformation
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:desktop_display;
                               owl:someValuesFrom  ocsf:Display
                             ];
        dcterms:description  "The Device Hardware Information object contains details and specifications of the physical components that make up a device. This information provides an overview of the hardware capabilities, configuration, and characteristics of the device.";
        ocsf:name            "device_hw_info" .

ocsf:Endpoint  rdf:type      owl:Class;
        rdfs:label           "Endpoint";
        rdfs:subClassOf      ocsf:Entity;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:location;
                               owl:someValuesFrom  ocsf:GeoLocation
                             ];
        dcterms:description  "The Endpoint object describes a physical or virtual device that connects to and exchanges information with a computer network. Some examples of endpoints are mobile devices, desktop computers, virtual machines, embedded devices, and servers. Internet-of-Things devices—like cameras, lighting, refrigerators, security systems, smart speakers, and thermostats—are also endpoints.";
        ocsf:constraints     "{\"at_least_one\":[\"ip\",\"uid\",\"name\",\"hostname\",\"instance_uid\",\"interface_uid\",\"interface_name\"]}";
        ocsf:name            "endpoint";
        ocsf:observable      20 .

ocsf:KillChainPhase  rdf:type  owl:Class;
        rdfs:label           "Kill Chain Phase";
        rdfs:subClassOf      ocsf:Object;
        dcterms:description  "The Kill Chain Phase object represents a single phase of a cyber attack, including the initial reconnaissance and planning stages up to the final objective of the attacker. It provides a detailed description of each phase and its associated activities within the broader context of a cyber attack. See <a target='_blank' href='https://www.lockheedmartin.com/en-us/capabilities/cyber/cyber-kill-chain.html'>Cyber Kill Chain®</a>.";
        ocsf:name            "kill_chain_phase" .

ocsf:Cloud  rdf:type         owl:Class;
        rdfs:label           "Cloud";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:org;
                               owl:someValuesFrom  ocsf:Organization
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:account;
                               owl:someValuesFrom  ocsf:Account
                             ];
        dcterms:description  "The Cloud object contains information about a cloud account such as AWS Account ID, regions, etc.";
        ocsf:name            "cloud" .

ocsf:DNSQuery  rdf:type      owl:Class;
        rdfs:label           "DNS Query";
        rdfs:subClassOf      ocsf:DNS;
        dcterms:description  "The DNS query object represents a specific request made to the Domain Name System (DNS) to retrieve information about a domain or perform a DNS operation. This object encapsulates the necessary attributes and methods to construct and send DNS queries, specify the query type (e.g., A, AAAA, MX). Defined by D3FEND <a target='_blank' href='https://d3fend.mitre.org/dao/artifact/d3f:DNSLookup/'>d3f:DNSLookup</a>.";
        ocsf:name            "dns_query" .

ocsf:RequestElements  rdf:type  owl:Class;
        rdfs:label           "Request Elements";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:containers;
                               owl:someValuesFrom  ocsf:Container
                             ];
        dcterms:description  "The Request Elements object describes characteristics of an API request.";
        ocsf:name            "request" .

ocsf:Job  rdf:type           owl:Class;
        rdfs:label           "Job";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:user;
                               owl:someValuesFrom  ocsf:User
                             ];
        rdfs:subClassOf      [ rdf:type         owl:Restriction;
                               owl:cardinality  1;
                               owl:onProperty   ocsf:file
                             ];
        dcterms:description  "The Job object provides information about a scheduled job or task, including its name, command line, and state. It encompasses attributes that describe the properties and status of the scheduled job.";
        ocsf:name            "job" .

ocsf:FindingInformation
        rdf:type             owl:Class;
        rdfs:label           "Finding Information";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:kill_chain;
                               owl:someValuesFrom  ocsf:KillChainPhase
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:attacks;
                               owl:someValuesFrom  ocsf:MITREATTCK
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:analytic;
                               owl:someValuesFrom  ocsf:Analytic
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:related_analytics;
                               owl:someValuesFrom  ocsf:Analytic
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:related_events;
                               owl:someValuesFrom  ocsf:RelatedEvent
                             ];
        dcterms:description  "The Finding Information object describes metadata related to a security finding generated by a security tool or system.";
        ocsf:name            "finding_info" .

ocsf:CISBenchmark  rdf:type  owl:Class;
        rdfs:label           "CIS Benchmark";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:cis_controls;
                               owl:someValuesFrom  ocsf:CISControl
                             ];
        dcterms:description  "The CIS Benchmark object describes best practices for securely configuring IT systems, software, networks, and cloud infrastructure as defined by the <a target='_blank' href='https://www.cisecurity.org/cis-benchmarks/'>Center for Internet Security</a>. See also <a target='_blank' href='https://www.cisecurity.org/insights/blog/getting-to-know-the-cis-benchmarks'>Getting to Know the CIS Benchmarks</a>.";
        ocsf:name            "cis_benchmark" .

ocsf:KBArticle  rdf:type     owl:Class;
        rdfs:label           "KB Article";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:product;
                               owl:someValuesFrom  ocsf:Product
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:os;
                               owl:someValuesFrom  ocsf:OperatingSystemOS
                             ];
        dcterms:description  "The KB Article object contains metadata that describes the patch or update.";
        ocsf:name            "kb_article" .

ocsf:Service  rdf:type       owl:Class;
        rdfs:label           "Service";
        rdfs:subClassOf      ocsf:Entity;
        dcterms:description  "The Service object describes characteristics of a service, <code> e.g. AWS EC2. </code>";
        ocsf:name            "service" .

ocsf:GeoLocation  rdf:type   owl:Class;
        rdfs:label           "Geo Location";
        rdfs:subClassOf      ocsf:Object;
        dcterms:description  "The Geo Location object describes a geographical location, usually associated with an IP address. Defined by D3FEND <a target='_blank' href='https://d3fend.mitre.org/dao/artifact/d3f:PhysicalLocation/'>d3f:PhysicalLocation</a>.";
        ocsf:constraints     "{\"at_least_one\":[\"coordinates\",\"city\",\"country\",\"postal_code\",\"region\"]}";
        ocsf:name            "location";
        ocsf:observable      26 .

ocsf:DigitalSignature
        rdf:type             owl:Class;
        rdfs:label           "Digital Signature";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:digest;
                               owl:someValuesFrom  ocsf:Fingerprint
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:certificate;
                               owl:someValuesFrom  ocsf:DigitalCertificate
                             ];
        dcterms:description  "The Digital Signature object contains information about the cryptographic mechanism used to verify the authenticity, integrity, and origin of the file or application.";
        ocsf:name            "digital_signature" .

ocsf:AffectedSoftwarePackage
        rdf:type             owl:Class;
        rdfs:label           "Affected Software Package";
        rdfs:subClassOf      ocsf:SoftwarePackage;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:remediation;
                               owl:someValuesFrom  ocsf:Remediation
                             ];
        dcterms:description  "The Affected Package object describes details about a software package identified as affected by a vulnerability/vulnerabilities.";
        ocsf:name            "affected_package" .

ocsf:Metric  rdf:type        owl:Class;
        rdfs:label           "Metric";
        rdfs:subClassOf      ocsf:Object;
        dcterms:description  "The Metric object defines a simple name/value pair entity for a metric.";
        ocsf:name            "metric" .

ocsf:CISCSC  rdf:type        owl:Class;
        rdfs:label           "CIS CSC";
        rdfs:subClassOf      ocsf:Object;
        dcterms:description  "The CIS Critical Security Control (CSC) contains information as defined by the Center for Internet Security Critical Security Control <a target='_blank' href='https://www.cisecurity.org/controls'>(CIS CSC)</a>. Prioritized set of actions to protect your organization and data from cyber-attack vectors.";
        ocsf:name            "cis_csc" .

ocsf:SubTechnique  rdf:type  owl:Class;
        rdfs:label           "Sub Technique";
        rdfs:subClassOf      ocsf:Entity;
        dcterms:description  "The Sub Technique object describes the sub technique ID and/or name associated to an attack, as defined by <a target='_blank' href='https://attack.mitre.org/wiki/ATT&CK_Matrix'>ATT&CK Matrix<sup>TM</sup></a>.";
        ocsf:name            "sub_technique" .

ocsf:EPSS  rdf:type          owl:Class;
        rdfs:label           "EPSS";
        rdfs:subClassOf      ocsf:Object;
        dcterms:description  "The Exploit Prediction Scoring System (EPSS) object describes the estimated probability a vulnerability will be exploited. EPSS is a community-driven effort to combine descriptive information about vulnerabilities (CVEs) with evidence of actual exploitation in-the-wild. (<a target='_blank' href='https://www.first.org/epss/'>EPSS</a>).";
        ocsf:name            "epss" .

ocsf:SoftwarePackage  rdf:type  owl:Class;
        rdfs:label           "Software Package";
        rdfs:subClassOf      ocsf:Object;
        dcterms:description  "The Software Package object describes details about a software package. Defined by D3FEND <a target='_blank' href='https://d3fend.mitre.org/dao/artifact/d3f:SoftwarePackage/'>d3f:SoftwarePackage</a>.";
        ocsf:name            "package" .

ocsf:User  rdf:type          owl:Class;
        rdfs:label           "User";
        rdfs:subClassOf      ocsf:Entity;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:account;
                               owl:someValuesFrom  ocsf:Account
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:ldap_person;
                               owl:someValuesFrom  ocsf:LDAPPerson
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:org;
                               owl:someValuesFrom  ocsf:Organization
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:groups;
                               owl:someValuesFrom  ocsf:Group
                             ];
        dcterms:description  "The User object describes the characteristics of a user/person or a security principal. Defined by D3FEND <a target='_blank' href='https://d3fend.mitre.org/dao/artifact/d3f:UserAccount/'>d3f:UserAccount</a>.";
        ocsf:constraints     "{\"at_least_one\":[\"account\",\"name\",\"uid\"]}";
        ocsf:name            "user";
        ocsf:observable      21 .

ocsf:Device  rdf:type        owl:Class;
        rdfs:label           "Device";
        rdfs:subClassOf      ocsf:Endpoint;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:location;
                               owl:someValuesFrom  ocsf:GeoLocation
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:org;
                               owl:someValuesFrom  ocsf:Organization
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:os;
                               owl:someValuesFrom  ocsf:OperatingSystemOS
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:network_interfaces;
                               owl:someValuesFrom  ocsf:NetworkInterface
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:hw_info;
                               owl:someValuesFrom  ocsf:DeviceHardwareInfo
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:image;
                               owl:someValuesFrom  ocsf:Image
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:groups;
                               owl:someValuesFrom  ocsf:Group
                             ];
        dcterms:description  "The Device object represents an addressable computer system or host, which is typically connected to a computer network and participates in the transmission or processing of data within the computer network. Defined by D3FEND <a target='_blank' href='https://d3fend.mitre.org/dao/artifact/d3f:Host/'>d3f:Host</a>.";
        ocsf:name            "device" .

ocsf:Metadata  rdf:type      owl:Class;
        rdfs:label           "Metadata";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type         owl:Restriction;
                               owl:cardinality  1;
                               owl:onProperty   ocsf:product
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:loggers;
                               owl:someValuesFrom  ocsf:Logger
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:extensions;
                               owl:someValuesFrom  ocsf:SchemaExtension
                             ];
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
                               owl:onProperty      ocsf:extension;
                               owl:someValuesFrom  ocsf:SchemaExtension
                             ];
        dcterms:description  "The Metadata object describes the metadata associated with the event. Defined by D3FEND <a target='_blank' href='https://d3fend.mitre.org/dao/artifact/d3f:Metadata/'>d3f:Metadata</a>.";
        ocsf:name            "metadata" .

ocsf:HTTPRequest  rdf:type   owl:Class;
        rdfs:label           "HTTP Request";
        rdfs:subClassOf      ocsf:Object;
        rdfs:subClassOf      [ rdf:type            owl:Restriction;
