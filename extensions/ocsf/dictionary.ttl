ocsf:cwe  rdf:type           rdf:Property;
        rdfs:label           "CWE";
        rdfs:range           ocsf:CWE;
        dcterms:description  "The CWE object represents a weakness in a software system that can be exploited by a threat actor to perform an attack. The CWE object is based on the <a target='_blank' href='https://cwe.mitre.org/'>Common Weakness Enumeration (CWE)</a> catalog." .

ocsf:email_t  rdf:type       rdfs:Datatype;
        rdfs:label           "Email Address";
        rdfs:subClassOf      xsd:string;
        dcterms:description  "Email address. For example: <code>john_doe@example.com</code>.";
        ocsf:observable      5;
