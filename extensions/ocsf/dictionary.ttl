        rdfs:range           ocsf:File;
        dcterms:description  "The folder that pertains to the event." .

ocsf:ja3s_hash  rdf:type     rdf:Property;
        rdfs:label           "JA3S Hash";
        rdfs:range           ocsf:Fingerprint;
        dcterms:description  "The MD5 hash of a JA3S string." .

ocsf:extension  rdf:type     rdf:Property;
        rdfs:label           "Schema Extension";
        rdfs:range           ocsf:SchemaExtension;
        dcterms:description  "The schema extension used to create the event." .

ocsf:extensions  rdf:type    rdf:Property;
        rdfs:label           "Schema Extensions";
        rdfs:range           ocsf:SchemaExtension;
