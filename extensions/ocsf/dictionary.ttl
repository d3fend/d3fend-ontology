        ocsf:is_array        true .

ocsf:server_hassh  rdf:type  rdf:Property;
        rdfs:label           "Server HASSH";
        rdfs:range           ocsf:HASSH;
        dcterms:description  "The Server HASSH fingerprinting object." .

ocsf:driver  rdf:type        rdf:Property;
        rdfs:label           "Kernel Driver";
        rdfs:range           ocsf:KernelExtension;
        dcterms:description  "The driver that was loaded/unloaded into the kernel" .

ocsf:device  rdf:type        rdf:Property;
        rdfs:label           "Device";
        rdfs:range           ocsf:Device;
        dcterms:description  "An addressable device, computer system or host." .

ocsf:integer_t  rdf:type     rdfs:Datatype;
        rdfs:label           "Integer";
        rdfs:subClassOf      xsd:integer;
        dcterms:description  "Signed integer value." .

ocsf:accessor  rdf:type      rdf:Property;
        rdfs:label           "Accessor";
        rdfs:range           ocsf:User;
        dcterms:description  "The name of the user who last accessed the object." .

ocsf:web_resources  rdf:type  rdf:Property;
        rdfs:label           "Web Resources";
