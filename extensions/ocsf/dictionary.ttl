ocsf:resource_uid_t  rdf:type  rdfs:Datatype;
        rdfs:label            "Resource UID";
        rdfs:subClassOf       xsd:string;
        dcterms:description   "Resource unique identifier. For example, S3 Bucket name or EC2 Instance ID.";
        ocsf:observable       10;
