ocsf:idp  rdf:type           rdf:Property;
        rdfs:label           "Identity Provider";
        rdfs:range           ocsf:IdentityProvider;
        dcterms:description  "This object describes details about the Identity Provider used." .

ocsf:org  rdf:type           rdf:Property;
        rdfs:label           "Organization";
        rdfs:range           ocsf:Organization;
        dcterms:description  "Organization and org unit relevant to the event or object." .

ocsf:account  rdf:type       rdf:Property;
        rdfs:label           "Account";
        rdfs:range           ocsf:Account;
        dcterms:description  "The account object describes details about the account that was the source or target of the activity." .

ocsf:entity  rdf:type        rdf:Property;
        rdfs:label           "Entity";
        rdfs:range           ocsf:ManagedEntity;
