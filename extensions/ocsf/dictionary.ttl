        dcterms:description  "The remote display affiliated with the event" .

ocsf:affected_code  rdf:type  rdf:Property;
        rdfs:label           "Affected Code";
        rdfs:range           ocsf:AffectedCode;
        dcterms:description  "List of Affected Code objects that describe details about code blocks identified as vulnerable.";
        ocsf:is_array        true .

