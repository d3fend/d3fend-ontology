ocsf:affected_packages
        rdf:type             rdf:Property;
        rdfs:label           "Affected Software Packages";
        rdfs:range           ocsf:AffectedSoftwarePackage;
        dcterms:description  "List of software packages identified as affected by a vulnerability/vulnerabilities.";
        ocsf:is_array        true .

ocsf:certificate  rdf:type   rdf:Property;
        rdfs:label           "Certificate";
        rdfs:range           ocsf:DigitalCertificate;
        dcterms:description  "The certificate object containing information about the digital certificate." .

ocsf:hw_info  rdf:type       rdf:Property;
        rdfs:label           "Hardware Info";
        rdfs:range           ocsf:DeviceHardwareInfo;
        dcterms:description  "The device hardware information." .

ocsf:ja3_hash  rdf:type      rdf:Property;
        rdfs:label           "JA3 Hash";
        rdfs:range           ocsf:Fingerprint;
        dcterms:description  "The MD5 hash of a JA3 string." .

ocsf:group  rdf:type         rdf:Property;
        rdfs:label           "Group";
        rdfs:range           ocsf:Group;
        dcterms:description  "The group object associated with an entity such as user, policy, or rule." .

ocsf:tactics  rdf:type       rdf:Property;
        rdfs:label           "Tactics";
