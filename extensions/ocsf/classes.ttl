ocsf:related_analytics
        rdf:type             rdf:Property;
        rdfs:label           "Related Analytics";
        rdfs:range           ocsf:Analytic;
        dcterms:description  "Describes analytics related to the analytic of a finding or detection as identified by the security product.";
        ocsf:is_array        true .

ocsf:manager  rdf:type       rdf:Property;
        rdfs:label           "Manager";
        rdfs:range           ocsf:User;
        dcterms:description  "The user's manager. This helps in understanding an org hierarchy. This should only ever be populated once in an event. I.e. there should not be a manager's manager in an event." .

ocsf:feature  rdf:type       rdf:Property;
        rdfs:label           "Feature";
        rdfs:range           ocsf:Feature;
        dcterms:description  "The feature that reported the event." .

ocsf:users  rdf:type         rdf:Property;
        rdfs:label           "Users";
        rdfs:range           ocsf:User;
        dcterms:description  "The users that pertain to the event or object.";
        ocsf:is_array        true .

ocsf:connection_info  rdf:type  rdf:Property;
        rdfs:label           "Connection Info";
        rdfs:range           ocsf:NetworkConnectionInformation;
        dcterms:description  "The network connection information." .

ocsf:ip_t  rdf:type           rdfs:Datatype;
        rdfs:label            "IP Address";
        rdfs:subClassOf       xsd:string;
        dcterms:description   "Internet Protocol address (IP address), in either IPv4 or IPv6 format. For example, <code>192.168.200.24</code> or <code>2001:0db8:85a3:0000:0000:8a2e:0370:7334</code>.";
        ocsf:observable       2;
        jsonschema:maxLength  40;
        jsonschema:pattern    "((^\\s*((([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5]).){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5]))\\s*$)|(^\\s*((([0-9A-Fa-f]{1,4}:){7}([0-9A-Fa-f]{1,4}|:))|(([0-9A-Fa-f]{1,4}:){6}(:[0-9A-Fa-f]{1,4}|((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){5}(((:[0-9A-Fa-f]{1,4}){1,2})|:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){4}(((:[0-9A-Fa-f]{1,4}){1,3})|((:[0-9A-Fa-f]{1,4})?:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){3}(((:[0-9A-Fa-f]{1,4}){1,4})|((:[0-9A-Fa-f]{1,4}){0,2}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){2}(((:[0-9A-Fa-f]{1,4}){1,5})|((:[0-9A-Fa-f]{1,4}){0,3}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){1}(((:[0-9A-Fa-f]{1,4}){1,6})|((:[0-9A-Fa-f]{1,4}){0,4}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(:(((:[0-9A-Fa-f]{1,4}){1,7})|((:[0-9A-Fa-f]{1,4}){0,5}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:)))(%.+)?\\s*$))" .

ocsf:groups  rdf:type        rdf:Property;
        rdfs:label           "Groups";
        rdfs:range           ocsf:Group;
        dcterms:description  "The groups to which an entity belongs. See specific usage.";
        ocsf:is_array        true .

ocsf:cloud  rdf:type         rdf:Property;
        rdfs:label           "Cloud";
        rdfs:range           ocsf:Cloud;
        dcterms:description  "Describes details about the Cloud environment where the event was originally created or logged." .

ocsf:extension_list  rdf:type  rdf:Property;
        rdfs:label           "Extension List";
        rdfs:range           ocsf:TLSExtension;
        dcterms:description  "The list of TLS extensions.";
        ocsf:is_array        true .

ocsf:remediation  rdf:type   rdf:Property;
        rdfs:label           "Remediation Guidance";
        rdfs:range           ocsf:Remediation;
        dcterms:description  "Describes the recommended remediation steps to address identified issue(s)." .

ocsf:owner  rdf:type         rdf:Property;
        rdfs:label           "Owner";
        rdfs:range           ocsf:User;
        dcterms:description  "The user that owns the file/object." .

ocsf:cvss  rdf:type          rdf:Property;
        rdfs:label           "CVSS Score";
        rdfs:range           ocsf:CVSSScore;
        dcterms:description  "The CVSS object details Common Vulnerability Scoring System (<a target='_blank' href='https://www.first.org/cvss/'>CVSS</a>) scores from the advisory that are related to the vulnerability.";
