        ocsf:is_array        true .

ocsf:hash  rdf:type          rdf:Property;
        rdfs:label           "Hash";
        rdfs:range           ocsf:Fingerprint;
        dcterms:description  "The hash attribute is the value of a digital fingerprint including information about its algorithm." .

ocsf:email_auth  rdf:type    rdf:Property;
        rdfs:label           "Email Authentication";
        rdfs:range           ocsf:EmailAuthentication;
